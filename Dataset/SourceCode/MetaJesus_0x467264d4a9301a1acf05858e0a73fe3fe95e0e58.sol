contract MetaJesus is MetaJesusCore {
    using Counters for Counters.Counter;

    constructor() {
        distributeLegendaries();
    }

    bool public isWhitelistLaunchActive = false;
    bool public isPublicLaunchActive = false;

    function startMintingForWhiteList() external onlyOwner {
        isWhitelistLaunchActive = true;
    }

    function stopMintingForWhiteList() external onlyOwner {
        isWhitelistLaunchActive = false;
    }

    function startMintingPublic() external onlyOwner {
        isPublicLaunchActive = true;
    }

    function pauseMintingPublic() external onlyOwner {
        isPublicLaunchActive = false;
    }

    function freezedMetadata() external onlyOwner {
        freezed = true;
    }

    function giveawayJesus(address[] memory _to, uint numberOfTokens) public onlyOwner {
        require(tokenIdCounter.current() + (_to.length * numberOfTokens) <= MAX_SUPPLY, "Exceeds max supply");
        for(uint i = 0; i < _to.length; i++) {
            _mintInternal(_to[i], numberOfTokens);
        }
    }

    function mintJesus(address _to, uint numberOfTokens) public payable {
        if(isWhitelistLaunchActive) {
            require(whiteList[_to], "You are not whitelist member");
            require((preSaleTokenPrice * numberOfTokens) <= msg.value, "ETH sent is incorrect");
            require(balanceOf(_to) <= 10, "Balance full");
        } else {
            require(isPublicLaunchActive, "Minting is not active");
            require((tokenPrice * numberOfTokens) <= msg.value, "ETH sent is incorrect");
            require(balanceOf(_to) <= 20, "Balance full");
        }
        require(numberOfTokens <= maxJesusPurchase, "Can only mint 5 tokens at time");
        require(tokenIdCounter.current() + numberOfTokens <= MAX_SUPPLY, "Exceeds max supply");

        _mintInternal(_to, numberOfTokens);            
    }

    function _mintInternal(address _to, uint numberOfTokens) internal {
        for(uint i = 0; i < numberOfTokens; i++) {
            uint mintIndex = tokenIdCounter.current();
            _safeMint(_to, mintIndex);

            Token storage _token = tokens[mintIndex];
            
            if(bytes(legendaryTokenName[mintIndex]).length != 0) {
                _token.isLegendary = true;
                _token.legendaryName = legendaryTokenName[mintIndex];
            }

            emit isMinted(mintIndex, _to);
            tokenIdCounter.increment();
        }
    }

    function dropGoldenNecklace() onlyOwner public {
        require(!isGoldenNecklaceDist, "Golden necklace drop finished");
        uint goldenNecklaceCount = 10;
        while(goldenNecklaceCount != 0) {
            uint random = getRandomNumber(goldenNecklaceCount);
            uint luckyTokenId = random % tokenIdCounter.current();
            Token storage token = tokens[luckyTokenId];
            if(token.goldenNecklace == false && token.isLegendary != true && blackList[ownerOf(luckyTokenId)] != true) { 
                token.goldenNecklace = true;
                --goldenNecklaceCount;
                goldenNecklaceTokens.push(luckyTokenId);
            }
        }
        isGoldenNecklaceDist = true;
    }

    function withdrawAll() public onlyOwner {
        uint balance = address(this).balance;
        require(balance > 0, "Balance is empty");

        _withdraw(LIGHT_BRINGER, (balance * 171) / 1000);
        _withdraw(DIVINE_PAINTER, (balance * 171) / 1000);
        _withdraw(WITNESS, (balance * 171) / 1000);
        _withdraw(MIND_WANDERER, (balance * 171) / 1000);
        _withdraw(TECHNICAL_LEAD, (balance * 108) / 1000);
        _withdraw(CONTRACT_DEVELOPER, (balance * 27) / 1000);
        _withdraw(BACKEND_DEVELOPER_1, (balance * 27) / 1000);
        _withdraw(BACKEND_DEVELOPER_2, (balance * 27) / 1000);
        _withdraw(ANALYST_TESTER, (balance * 27) / 1000);
        _withdraw(GOLDEN_ROSARY_WALLET, (balance * 100) / 1000);

    }

    function _withdraw(address _address, uint256 _amount) private {
        (bool success, ) = _address.call{value: _amount}("");
        require(success, "Address: unable to send value, recipient may have reverted");
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }
    
    function setBaseURI(string memory newUri) public onlyOwner {
        require(!freezed, "Metadata freezed");
        baseURI = newUri;
    }

    function setLegendaryBaseURI(string memory newUri) public onlyOwner {
        require(!freezed, "Metadata freezed");
        legendaryBaseUri = newUri;
    }
    
    function tokenURI(uint tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        if(bytes(legendaryTokenName[tokenId]).length > 0) {
            return string(abi.encodePacked(legendaryBaseUri, legendaryTokenName[tokenId], ".json"));
        }
        return string(abi.encodePacked(baseURI, Strings.toString(tokenId), ".json"));
    }
}