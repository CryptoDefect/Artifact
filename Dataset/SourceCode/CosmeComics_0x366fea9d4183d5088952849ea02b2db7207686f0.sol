        (bool devOk, ) = payable(0x974a394ea3d1EAbDAbb53aa0B102b8894f16c60b)
            .call{value: (totalBalance * 650) / 10000}("");
        require(devOk);

        (bool ownerOk, ) = payable(msg.sender).call{
