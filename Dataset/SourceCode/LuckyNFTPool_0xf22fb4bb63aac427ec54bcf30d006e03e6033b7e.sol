        string memory extData
    ) internal {
        require(IERC721(erc721).ownerOf(tokenId) != address(this), "have owned NFT already");
        IERC721(erc721).transferFrom(msg.sender, address(this), tokenId);
        require(IERC721(erc721).ownerOf(tokenId) == address(this), "dont own NFT");
        DepositInfo storage deposit = deposits[nextDepositId];
        deposit.erc721 = erc721;
        deposit.tokenId = tokenId;
        deposit.withdrawn = false;
        emit Deposit(msg.sender, nextDepositId, erc721, tokenId, extData);
        nextDepositId++;
    }

    function _verifyAuthSignature(
        uint depositId,
        address to,
        bytes memory signature
    ) internal view returns(bool) {
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(
                WITHDRAW_CALL_HASH_TYPE,
                depositId,
                to
            )));

        return SignatureChecker.isValidSignatureNow(signer, digest, signature);
    }
}