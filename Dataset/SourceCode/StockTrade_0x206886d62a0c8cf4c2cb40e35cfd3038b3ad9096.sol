      "content": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.0;\n\nimport {IPermissionManager} from \"./interfaces/IPermissionManager.sol\";\n\n/// @title Permissioned\n/// @author DeepchainLabs\n/// @notice This contract provides a modifier for permissioned functions\n/// @dev This contract is intended to be inherited by any contract that needs to restrict access to certain functions\ncontract Permissioned {\n    /// @notice The address of the permission manager contract\n    address private permissionManager;\n\n    event PermissionManagerUpdated(address indexed permissionManager);\n\n    /// @notice Initializes the permission manager address\n    /// @dev This function should be called in the initializer function or constructor of the contract\n    /// @param _permissionManager (address) The address of the permission manager contract\n    function initializePermissioned(address _permissionManager) internal {\n        require(\n            _permissionManager != address(0),\n            \"Invalid permission manager address\"\n        );\n        permissionManager = _permissionManager;\n    }\n\n    /// @dev Modifier to be used in permissioned functions that only admins or managers can call\n    modifier onlyAdminOrManager() {\n        require(\n            IPermissionManager(permissionManager).isManagerOrAdmin(msg.sender),\n            \"Only admin or manager\"\n        );\n        _;\n    }\n\n    /// @dev Modifier to be used in permissioned functions that only admins can call\n    modifier onlyAdmin() {\n        require(\n            IPermissionManager(permissionManager).isAdmin(msg.sender),\n            \"Only admin\"\n        );\n        _;\n    }\n\n    /// @dev Modifier to be used in permissioned functions that only managers can call\n    modifier onlyManager() {\n        require(\n            IPermissionManager(permissionManager).isManager(msg.sender),\n            \"Only manager\"\n        );\n        _;\n    }\n\n    /// @notice Updates the permission manager address\n    /// @dev This function can only be called by an admin\n    /// @param _permissionManager (address)\n    function updatePermissionManagerAddress(\n        address _permissionManager\n    ) external onlyAdmin {\n        require(\n            _permissionManager != address(0),\n            \"Invalid permission manager address\"\n        );\n        require(\n            IPermissionManager(_permissionManager).isAdmin(msg.sender),\n            \"Changing manager without new permission\"\n        );\n        permissionManager = _permissionManager;\n    }\n\n    /// @notice Returns the address of the permission manager contract\n    /// @return  (address) The address of the permission manager contract\n    function getPermissionManagerAddress() public view returns (address) {\n        return permissionManager;\n    }\n}\n"

    },

