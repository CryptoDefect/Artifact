    for (uint256 i = 0; i < ids.length; i++) {
        require(itemDetails[ids[i]].maxSupply == 0, "Item already exists");
        itemDetails[ids[i]] = ItemDetails(prices_eth[i], prices_catgems[i], maxSupply[i], chance[i]);
        _itemsByCategory[category[i]].push(ids[i]); 
    }
  }

  function updatePrice(uint256[] calldata ids, uint256[] calldata prices_eth, uint256[] calldata prices_catgems) external onlyOwner {
    for (uint256 i = 0; i < ids.length; i++) {
        require(itemDetails[ids[i]].maxSupply > 0, "Item doesn't exist");
        itemDetails[ids[i]].price_eth = prices_eth[i];
        itemDetails[ids[i]].price_catgems = prices_catgems[i];
    }
  }

  function setURI(string calldata uri_) public virtual onlyOwner {
    _setURI(uri_);
  }

  function totalSupply(
    uint256 id
  ) public view returns (uint256) {
    return _tokenSupply[id];
  }

  function randomNum(uint i) private view returns (uint256) {
    return uint256(keccak256(abi.encodePacked(blockhash(block.number - i), msg.sender, block.difficulty, block.timestamp, _mintedCatsCount)));
  }

  function setPresaleStartTime(uint256 newStartTime) external onlyOwner {
    presaleStartTime = newStartTime;
  }
  
  function setPublicSaleStartTime(uint256 newStartTime) external onlyOwner {
    publicSaleStartTime = newStartTime;
  }

  function setCatGemsPrice(uint256 newCatGemsPrice) external onlyOwner {
    catGemsPerCat = newCatGemsPrice * 10**18;
  }

  function setEthPrice(uint256 newEthPrice) external onlyOwner {
    catPrice = newEthPrice;
  }

  function adminMint1of1(address to, uint256 id) external onlyOwner {
    require(_nft_contract_set, "NFT Contract not set");
    require(_tokenSupply[id] == 0, "ID already minted");
    require(id >= 1 && id <= 6, "ID must be between 1 and 6");

    _nft_contract.mint(to, id);
    _tokenSupply[id]++;
  }

  function adminMint(address to, uint256 qty) external onlyOwner {
      require(_nft_contract_set, "NFT Contract not set");
      require(qty <= _maxMintsPerTransaction, "You cannot mint that many cats in one transaction");
      require(_mintedCatsCount + qty <= _catsMaxSupply, "Insufficient number of cats on stock");
      for (uint256 i = 0; i < qty; i++)
        mintRandomCat(to);
  }

  function withdraw() external onlyOwner {
    (bool success, ) = payable(msg.sender).call{value: address(this).balance}('');
    require(success, "Failure! Ether not sent.");
  }

  function buyCatForGenesisHolders(uint256 qty) external {
    require(!_mintPaused, "Minting is paused");
    require(block.timestamp >= presaleStartTime, "Presale not live");
    require(_nft_contract_set, "NFT Contract not set");
    require(_holdersMintCount + qty <= _catsMaxSupplyGenesisHolders, "Quanitity is over the genesis holders supply");
    require(_mintedCatsCount + qty <= _catsMaxSupply, "Insufficient number of cats on stock");
    require(presaleMintCountByAddress[msg.sender].genesisMintCount + qty <= genesisMaxMintsPerAddress, "Genesis holders max mints reached");
    require(presaleMintCountByAddress[msg.sender].genesisMintCount + qty <= _genesis_contract.balanceOf(msg.sender), "Quanitity is over the genesis holder limit");
    require(_catgems_contract.transferFrom(msg.sender, address(this), qty * catGemsPerCat), "Unsuccessful CatGems transaction");

    for(uint256 i = 0; i < qty; i++)
        mintRandomCat(msg.sender);
    
    _holdersMintCount += qty;
    presaleMintCountByAddress[msg.sender].genesisMintCount += uint8(qty);
  }
  
  function buyCatWhitelist(uint256 qty, Coupon calldata coupon) external payable {
    require(!_mintPaused, "Minting is paused");
    require(block.timestamp >= presaleStartTime, "Presale not live");
    require(_nft_contract_set, "NFT Contract not set");
    require(_whitelistMintCount + qty <= _catsMaxSupplyWhitelist, "Quanitity is over the whitelist supply.");
    require(_mintedCatsCount + qty <= _catsMaxSupply, "Insufficient number of cats on stock");
    require(presaleMintCountByAddress[msg.sender].whitelistMintCount + qty <= whitelistMaxMintsPerAddress, "Whitelist max mints reached");
    require(qty * catPrice == msg.value, "Incorrect amount of ether");
    
    bytes32 digest = keccak256(abi.encode(msg.sender));
    address signer = ecrecover(digest, coupon.v, coupon.r, coupon.s);
    require(signer != address(0), "ECDSA: invalid signature");
    require(signer == _couponSignerAddress, "Invalid coupon");

    for(uint256 i = 0; i < qty; i++)
        mintRandomCat(msg.sender);

    _whitelistMintCount += qty;
    presaleMintCountByAddress[msg.sender].whitelistMintCount += uint8(qty);
  }

  function buyCat(uint256 qty) external payable {
      require(block.timestamp >= publicSaleStartTime, "Public sale not live");
      require(qty <= _maxMintsPerTransaction, "You cannot mint that many cats in one transaction");
      require(!_mintPaused, "Minting is paused");
      require(_mintedCatsCount + qty <= _catsMaxSupply, "Insufficient number of cats in stock");
      require(qty * catPrice == msg.value, "Incorrect amount of ether");
      require(_nft_contract_set, "NFT Contract not set");

      for(uint256 i = 0; i < qty; i++)
          mintRandomCat(msg.sender);
  }

  function mintRandomCat(address to) private {
    require(to != address(0), "Cannot mint to 0 address");
    _mintedCatsCount++;
    uint catID = _mintedCatsCount;
    for (uint8 i = 1; i <= 4; i++) {
      uint pct = randomNum(i) % 10000 + 1;
      uint limit = 0;
      for (uint j = 0; j < _itemsByCategory[i].length; j++) {
        uint itemID = _itemsByCategory[i][j];
        ItemDetails memory itemData = itemDetails[itemID];
        
        if (pct > limit && pct <= limit + itemData.chance) {
          if (_tokenSupply[itemID] < itemData.maxSupply) {
            catID += itemID;
            _tokenSupply[itemID]++;
          }          
          break;
        }

        limit += itemData.chance;
      }
    }

    _tokenSupply[catID]++;
    _nft_contract.mint(to, catID);
  }

  function buyItems(uint256[] calldata ids, uint256[] calldata quantities) external payable {
    require(!_buyItemsPaused, "Buying items is paused");
    uint totalPriceETH = 0;
    uint totalPriceCatGems = 0;
    for(uint i = 0; i < ids.length; i++) {
      require(_tokenSupply[ids[i]] + quantities[i] <= itemDetails[ids[i]].maxSupply, "Not enough items in stock");
     
      _mint(msg.sender, ids[i], quantities[i], "");
      _tokenSupply[ids[i]] = _tokenSupply[ids[i]] + quantities[i];

      totalPriceETH += itemDetails[ids[i]].price_eth * quantities[i];
      totalPriceCatGems += itemDetails[ids[i]].price_catgems * 10 ** 18 * quantities[i];
    }
    
    require(msg.value == totalPriceETH, "Incorrect amount of ether");
    require(_catgems_contract.transferFrom(msg.sender, address(this), totalPriceCatGems), "Unsuccessful CatGems transaction");
  }

   function replaceItems(uint256 original_id, uint256 new_id) external {
      require(!_replaceItemsPaused, "Replacing items is paused");
      require(_nft_contract_set, "NFT Contract not set");
      require(_nft_contract.ownerOf(original_id) == msg.sender, "You are not the owner");
      require(original_id != new_id, "You must replace at least one item");
      require((original_id & 0xffff) == (new_id & 0xffff), "You cannot replace items on a different cat");
      require((original_id & 0xffff) > 6, "You cannot use items with 1 of 1 cats.");

      uint256 headwearEquipped = original_id & 0xffff0000;
      uint256 eyewearEquipped = original_id & 0xffff00000000;
      uint256 neckwearEquipped = original_id & 0xffff000000000000;
      uint256 clothesEquipped = original_id & 0xffff0000000000000000;

      uint256 headwearNew = new_id & 0xffff0000;
      uint256 eyewearNew = new_id & 0xffff00000000;
      uint256 neckwearNew = new_id & 0xffff000000000000;
      uint256 clothesNew = new_id & 0xffff0000000000000000; 

      if (headwearNew != headwearEquipped) {
        if (headwearNew != 0) {
          require(balanceOf(msg.sender, headwearNew) > 0, "You don't own the headwear");
          _burn(msg.sender, headwearNew, 1);
        }

        if (headwearEquipped != 0)
          _mint(msg.sender, headwearEquipped, 1, "");
      }

      if (eyewearNew != eyewearEquipped) {
        if (eyewearNew != 0) {
          require(balanceOf(msg.sender, eyewearNew) > 0, "You don't own the eyewear");
          _burn(msg.sender, eyewearNew, 1);
        }

        if (eyewearEquipped != 0)
          _mint(msg.sender, eyewearEquipped, 1, "");
      }

      if (neckwearNew != neckwearEquipped) {
        if (neckwearNew != 0) {
          require(balanceOf(msg.sender, neckwearNew) > 0, "You don't own the neckwear");
          _burn(msg.sender, neckwearNew, 1);
        }

        if (neckwearEquipped != 0)
          _mint(msg.sender, neckwearEquipped, 1, "");
      }

      if (clothesNew != clothesEquipped) {
        if (clothesNew != 0) {
          require(balanceOf(msg.sender, clothesNew) > 0, "You don't own the clothes");
          _burn(msg.sender, clothesNew, 1);
        }

        if (clothesEquipped != 0)
          _mint(msg.sender, clothesEquipped, 1, "");
      }

      _nft_contract.burn(original_id);
      _tokenSupply[original_id]--;

      _nft_contract.mint(msg.sender, new_id);
      _tokenSupply[new_id]++;
  }
}