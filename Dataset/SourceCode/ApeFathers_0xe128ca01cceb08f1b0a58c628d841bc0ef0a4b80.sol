   * @notice Error messages for burnClaim
   */
  error TokenCountVerificationFailed();
  error BurnClaimIsNotActive();
  error InvalidClaim();
  error OwnerOfTokenCallFailed(address);
  error CannotClaimTokenYouDontHold(uint256);
  error BurnFailed(uint256);

  /**
   * @dev Burn ApeDads Tokens and Receive ApeFathers tokens in return - WARNING: THIS ACTION IS IRREVERSABLE. ENSURE YOU ARE AWARE OF THE RISKS OF BURNING YOUR ASSETS.
   * @notice WARNING - IF YOU HAVE NOT APPROVED THIS CONTRACT ADDRESS TO THE NFT ADDRESS OF THE TOKEN TO BURN, YOU WILL HAVE A REVERTED TRANSACTION WITH LOSS OF GAS.
   * @notice WARNING - IF YOU DO NOT OWN A TOKEN FROM THE CONTRACT ADDRESS OF THE TOKEN TO BURN, YOU WILL HAVE A REVERTED TRANSACTION WITH LOSS OF GAS.
   * @notice ApeFathers and GSKNNFT Inc. will not be held accountable for any loss of transactional funds by using this function.
   * @notice ApeFathers and GSKNNFT Inc. will not be liable to reimburse any funds lost by the user by activating this function on their (the user's) own accord.
   * @notice By interacting with this function, you hereby absolve GSKNNFT Inc. and ApeFathers from any loss of funds, and aknowledge this is including transactional gas and any assets.
   * @param _tokenIds - array of token IDs to burn
   * @param verifyNumberOfTokens - number of tokens to verify
   */
  function burnClaim(uint256[] calldata _tokenIds, uint256 verifyNumberOfTokens) public {
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    if (_tokenIds.length != verifyNumberOfTokens) {
      revert TokenCountVerificationFailed();
    }
    if (!dds.isBurnClaimActive) {
      revert BurnClaimIsNotActive();
    }
    if (_tokenIds.length < 1) {
      revert InvalidClaim();
    }

    for (uint256 i = 0; i < _tokenIds.length; i++) {
      bytes memory ownerOfData = abi.encodeWithSignature("ownerOf(uint256)", _tokenIds[i]);
      (bool success, bytes memory returnData) = address(dds.nftAddress).staticcall(ownerOfData);
      if (!success) {
        revert OwnerOfTokenCallFailed(_msgSender());
      }
      address owner = abi.decode(returnData, (address));
      if (owner != _msgSender()) {
        revert CannotClaimTokenYouDontHold(_tokenIds[i]);
      }

      bytes memory burnData = abi.encodeWithSignature("burn(uint256)", _tokenIds[i]);
      (success, ) = address(dds.nftAddress).call(burnData);
      if (!success) {
        revert BurnFailed(_tokenIds[i]);
      }
    }
    _safeMint(_msgSender(), _tokenIds.length);
    emit BurnClaimComplete(address(_msgSender()), _tokenIds.length, _tokenIds);
  }

  /**
   * @dev Used to directly approve a token for transfers by the current _msgSender()
   */
  function _directApproveMsgSenderFor(uint256 tokenId) internal virtual {
    assembly {
      mstore(0x00, tokenId)
      mstore(0x20, 6) // '_tokenApprovals' is at slot 6.
      sstore(keccak256(0x00, 0x40), caller())
    }
  }

  /**
   * @dev Hook before token transfers
   */
  function beforeTokenTransfers(address from, address to, uint256 tokenId, uint256) internal virtual nonReentrant {
    if (from == address(0) || to == address(0) || from != to) {
      _directApproveMsgSenderFor(tokenId);
    }
  }

  /**
   * @notice Withdraws all funds held within contract
   */
  function withdraw() external nonReentrant {
    LibDiamond.enforceIsContractOwner();
    require(address(this).balance > 0, "NO_FUNDS");
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    uint256 balance = address(this).balance;
    for (uint256 i = 0; i < dds.payoutAddresses.length; i++) {
      require(payable(dds.payoutAddresses[i]).send((balance * dds.payoutBasisPoints[i]) / 10000));
    }
  }

  /**
   * @dev Uses multicall efficiently to execute multiple calls in a single transaction
   */
  function multicall(bytes[] calldata data, address diamond) external {
    diamond = diamond == address(0) ? address(this) : diamond;
    require(data.length > 0, "NO_DATA");

    for (uint256 i = 0; i < data.length; i++) {
      bytes memory callData = data[i];
      assembly {
        let result := delegatecall(gas(), diamond, add(callData, 0x20), mload(callData), 0, 0)
        if iszero(result) {
          revert(0, 0)
        }
      }
    }
  }

  /**
   * @dev Fallback function to allow for proxy delegation of calls to diamond.
   * @notice The receive function is called when ether is sent to the contract address.
   * @notice Any eth sent to this contract or any facet will be forwarded to the diamond
   */
  fallback() external payable {
    LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();

    bytes32 position = LibDiamond.DIAMOND_STORAGE_POSITION;
    assembly {
      ds.slot := position
    }
    address facet = address(bytes20(ds.facets[msg.sig]));
    require(facet != address(0), "NO_FUNC");

    assembly {
      calldatacopy(0, 0, calldatasize())
      let result := delegatecall(gas(), facet, 0, calldatasize(), 0, 0)
      let size := returndatasize()
      returndatacopy(0, 0, returndatasize())

      switch result
      case 0 {
        revert(0, returndatasize())
      }
      default {
        return(0, returndatasize())
      }
    }
  }

  receive() external payable {
    emit Received(_msgSender(), msg.value);
  }
