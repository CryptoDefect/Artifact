// File: scripts/casino.sol



pragma solidity ^0.8.0;





interface IERC20 {
    function transfer(address recipient, uint256 amount)
        external
        returns (bool);

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);

    function balanceOf(address account) external view returns (uint256);

    function approve(address spender, uint256 amount) external returns (bool);

    function allowance(address owner, address spender)
        external
        view
        returns (uint256);
}

contract WGCasino is AccessControlEnumerable, ReentrancyGuard {
    using SafeMath for uint256;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant CASINO_OWNER_ROLE = keccak256("CASINO_OWNER_ROLE");

    address public deployer;
    mapping(address => uint256) public secureDepositBalance;
    mapping(address => mapping(address => uint256)) public totalWinningsDeposit;
    mapping(address => mapping(address => uint256)) public playerDeposit;
    mapping(address => address[]) public playerList;
    mapping(address => mapping(address => bool)) public casinoPlayerMappings;

    IERC20 public token;

    constructor(address _tokenAddress) {
        deployer = msg.sender;
        token = IERC20(_tokenAddress);
        _setupRole(ADMIN_ROLE, deployer);
        _setupRole(DEFAULT_ADMIN_ROLE, deployer);
    }

    modifier onlyAdmin() {
        require(
            hasRole(ADMIN_ROLE, msg.sender),
            "Only admins can call this function."
        );
        _;
    }

    modifier onlyCasinoOwner() {
        require(
            hasRole(CASINO_OWNER_ROLE, msg.sender),
            "Only casino owners can call this function."
        );
        _;
    }

    function addAdmin(address _newAdmin) public onlyAdmin {
        grantRole(ADMIN_ROLE, _newAdmin);
    }

    function removeAdmin(address _admin) public onlyAdmin {
        revokeRole(ADMIN_ROLE, _admin);
    }

    function addCasinoOwner(address _newOwner) public onlyAdmin {
        grantRole(CASINO_OWNER_ROLE, _newOwner);
    }

    function removeCasinoOwner(address _owner) public onlyAdmin {
        revokeRole(CASINO_OWNER_ROLE, _owner);
    }

    function depositToSecure(uint256 _amount) public onlyCasinoOwner {
        require(_amount > 0, "Deposit amount must be greater than zero.");
        require(
            token.balanceOf(msg.sender) >= _amount,
            "Insufficient balance."
        );
        require(
            token.allowance(msg.sender, address(this)) >= _amount,
            "Token allowance not set."
        );

        bool success = token.transferFrom(msg.sender, address(this), _amount);
        require(success, "Token transfer failed.");

        secureDepositBalance[msg.sender] = secureDepositBalance[msg.sender].add(
            _amount
        );
    }

    function depositToPlayer(uint256 _amount, address _casinoOwner) public {
        require(_amount > 0, "Deposit amount must be greater than zero.");
        require(
            token.balanceOf(msg.sender) >= _amount,
            "Insufficient balance."
        );
        require(
            hasRole(CASINO_OWNER_ROLE, _casinoOwner),
            "Invalid casino owner."
        );

        bool success = token.transferFrom(msg.sender, address(this), _amount);
        require(success, "Token transfer failed");
        playerDeposit[_casinoOwner][msg.sender] = playerDeposit[_casinoOwner][
            msg.sender
        ].add(_amount);
        if (!casinoPlayerMappings[_casinoOwner][msg.sender]) {
            playerList[_casinoOwner].push(msg.sender);
            casinoPlayerMappings[_casinoOwner][msg.sender] = true;
        }
    }

    function moveFromPlayerToCasino(
        address _casinoOwner,
        address _player,
        uint256 _amount
    ) public onlyAdmin {
        require(
            hasRole(CASINO_OWNER_ROLE, _casinoOwner),
            "Invalid casino owner."
        );

        require(
            playerDeposit[_casinoOwner][_player] >= _amount,
            "Insufficient funds."
        );

        playerDeposit[_casinoOwner][_player] = playerDeposit[_casinoOwner][
            _player
        ].sub(_amount);
        totalWinningsDeposit[_casinoOwner][_player] = totalWinningsDeposit[
            _casinoOwner
        ][_player].add(_amount);
    }

    function refundAllPlayerDeposits(address _player) public {
        uint256 totalRefundAmount = 0;

        for (uint256 i = 0; i < getRoleMemberCount(CASINO_OWNER_ROLE); i++) {
            address casinoOwner = getRoleMember(CASINO_OWNER_ROLE, i);
            uint256 playerBalance = playerDeposit[casinoOwner][_player];

            if (playerBalance > 0) {
                totalRefundAmount = totalRefundAmount.add(playerBalance);
            }
        }

        require(totalRefundAmount > 0, "No deposits to refund.");

        bool success = token.transfer(_player, totalRefundAmount);
        require(success, "Token transfer failed.");

        for (uint256 i = 0; i < getRoleMemberCount(CASINO_OWNER_ROLE); i++) {
            address casinoOwner = getRoleMember(CASINO_OWNER_ROLE, i);
            uint256 playerBalance = playerDeposit[casinoOwner][_player];

            if (playerBalance > 0) {
                playerDeposit[casinoOwner][_player] = 0;
            }
        }
    }

    function moveFromCasinoToPlayer(
        address _casinoOwner,
        address _player,
        uint256 _amount
    ) public onlyAdmin {
        require(
            hasRole(CASINO_OWNER_ROLE, _casinoOwner),
            "Invalid casino owner."
        );

        require(
            secureDepositBalance[_casinoOwner] >= _amount,
            "Insufficient funds in the casino deposit."
        );

        uint256 winningsDepositBalance = totalWinningsDeposit[_casinoOwner][
            _player
        ];

        if (winningsDepositBalance < _amount) {
            // Calculate the amount of tokens needed from the secure deposit
            uint256 amountNeeded = _amount.sub(winningsDepositBalance);

            require(
                secureDepositBalance[_casinoOwner] >= amountNeeded,
                "Insufficient funds in the casino's secure deposit."
            );

            secureDepositBalance[_casinoOwner] = secureDepositBalance[
                _casinoOwner
            ].sub(amountNeeded);
            totalWinningsDeposit[_casinoOwner][_player] = totalWinningsDeposit[
                _casinoOwner
            ][_player].add(amountNeeded);
        }

        playerDeposit[_casinoOwner][_player] = playerDeposit[_casinoOwner][
            _player
        ].add(_amount);
        totalWinningsDeposit[_casinoOwner][_player] = totalWinningsDeposit[
            _casinoOwner
        ][_player].sub(_amount);
    }

    function sendToCasinoOwnerFromSecure(address _casinoOwner)
        public
        onlyAdmin
    {
        require(
            hasRole(CASINO_OWNER_ROLE, _casinoOwner),
            "Invalid casino owner."
        );
        require(
            secureDepositBalance[_casinoOwner] > 0,
            "Secure deposit balance is zero."
        );

        uint256 amount = secureDepositBalance[_casinoOwner];
        bool success = token.transfer(_casinoOwner, amount);
        require(success, "Token transfer failed");

        secureDepositBalance[_casinoOwner] = 0;
    }

    function sendToCasinoOwnerFromWinnings(address payable _casinoOwner)
        public
        onlyAdmin
        nonReentrant
    {
        require(
            hasRole(CASINO_OWNER_ROLE, _casinoOwner),
            "Invalid casino owner."
        );

        uint256 totalAmount = 0;
        for (uint256 i = 0; i < playerList[_casinoOwner].length; i++) {
            address player = playerList[_casinoOwner][i];
            totalAmount = totalAmount.add(
                totalWinningsDeposit[_casinoOwner][player]
            );
            totalWinningsDeposit[_casinoOwner][player] = 0;
        }
        require(totalAmount > 0, "Insufficient winnings deposit balance.");
        // Check contract's token balance before initiating transfer
        require(
            token.balanceOf(address(this)) >= totalAmount,
            "Insufficient contract balance."
        );

        bool success = token.transfer(_casinoOwner, totalAmount);
        require(success, "Token transfer failed.");
    }

    function getPlayerDeposit(address _casinoOwner, address _player)
        public
        view
        returns (uint256)
    {
        require(
            msg.sender == deployer ||
                msg.sender == _casinoOwner ||
                msg.sender == _player,
            "Caller must be deployer, casino owner, or player."
        );

        return playerDeposit[_casinoOwner][_player];
    }

    function getTotalPlayerDepositsForCasino(address _casinoOwner)
        public
        view
        returns (uint256)
    {
        require(
            msg.sender == deployer || msg.sender == _casinoOwner,
            "Caller must be deployer or casino owner."
        );
        uint256 totalDeposits = 0;
        for (uint256 i = 0; i < playerList[_casinoOwner].length; i++) {
            address player = playerList[_casinoOwner][i];
            totalDeposits = totalDeposits.add(
                playerDeposit[_casinoOwner][player]
            );
        }
        return totalDeposits;
    }

    function getTotalWinningsForCasino(address _casinoOwner)
        public
        view
        returns (uint256)
    {
        require(
            msg.sender == deployer || msg.sender == _casinoOwner,
            "Caller must be deployer or casino owner."
        );
        uint256 totalWinnings = 0;
        for (uint256 i = 0; i < playerList[_casinoOwner].length; i++) {
            address player = playerList[_casinoOwner][i];
            totalWinnings = totalWinnings.add(
                totalWinningsDeposit[_casinoOwner][player]
            );
        }
        return totalWinnings;
    }

    function getPlayerWinningsForCasino(address _casinoOwner, address _player)
        public
        view
        returns (uint256)
    {
        require(
            msg.sender == deployer ||
                msg.sender == _casinoOwner ||
                msg.sender == _player,
            "Caller must be deployer, casino owner, or player."
        );
        return totalWinningsDeposit[_casinoOwner][_player];
    }
}