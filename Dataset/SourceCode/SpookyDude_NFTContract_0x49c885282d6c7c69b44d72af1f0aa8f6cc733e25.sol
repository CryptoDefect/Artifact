// File: contracts/SpookyDude_Contract.sol



pragma solidity >=0.8.0 <0.9.0;





contract SpookyDude_NFTContract is ERC721A, Ownable{
    using Strings for uint256;

    uint256 public constant maxSupply = 3110;
    uint256 public OG_TokenQty = 210;
    uint256 public WL_TokenQty = 2000;

    uint256 public OG_TokenSold = 0;
    uint256 public WL_TokenSold = 0;

    uint256 public PRICE_OG = 0 ether;
    uint256 public PRICE_WL = 0.0195 ether;
    uint256 public PRICE_Public = 0.0495 ether; 
    
    uint256 public mintLimit_OG = 1;
    uint256 public mintLimit_WL = 2;
    uint256 public maxMintPerTx = 5;

    uint256 public Mint_State = 0;
    uint256 public contractBalance = 0;

    mapping(address => uint256) public OG_Claimed;
    mapping(address => uint256) public WL_Claimed;
    mapping(address => uint256) public public_Claimed;

    bytes32 public merkleRoot1;
    bytes32 public merkleRoot2;

    string public uriSuffix = ".json";
    string public baseURI = "";

    address payable private community = payable(0xb8798126321eaBB049565A40A95c4810a09EAf9d); //community fund
    address payable private investor_1 = payable(0x98A4434b3BBE43f9031e026dF88E1Bb864246420); //cjy
    address payable private investor_2 = payable(0xf018C4f9def7EF458A73e86E3B8dCA697f23Ebf1); //crazydude
    address payable private investor_3 = payable(0x49E7B31C18ee3FE8214C100949c242221884d2CC); //jeff
    address payable private dev_team = payable(0x84B29f70D92Ad3D2570830dEd557FafB1bfD608e); //dev salary + future r&d
    address payable private manager = payable(0x7d3A292445018D2aaFaf781bE091D6c58a71c52A); //wayne
    address payable private marketing_director = payable(0x268B443b0dE2944D7dCC506e13Aa9BD150450372); //azeez


    constructor(
        string memory _tokenName,
        string memory _tokenSymbol,
        string memory _MetadataUri
    ) ERC721A(_tokenName, _tokenSymbol) {
        setBaseURI(_MetadataUri);
        _safeMint(community, 100);
    }

    //Mint function
    function mint_OG(uint256 _mintAmount, bytes32[] memory _proof) public payable{
        require(Mint_State == 1, "OG mint is not actived");
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_proof, merkleRoot1, leaf), "Invalid proof!");
        require(OG_Claimed[msg.sender] + _mintAmount <= mintLimit_OG, "Address acquired max amout of OG NFT");
        require(OG_TokenSold + _mintAmount <= OG_TokenQty, "Purchase would exceed OG supply");
        require(msg.value >= PRICE_OG * _mintAmount, "Insufficient funds");
        OG_Claimed[msg.sender] += _mintAmount;
        _safeMint(msg.sender, _mintAmount);
        OG_TokenSold += _mintAmount;
    }

    function mint_WL(uint256 _mintAmount, bytes32[] memory _proof) public payable {
        require(Mint_State == 2, "WL mint is not actived");
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_proof, merkleRoot2, leaf), "Invalid proof!");
        require(WL_Claimed[msg.sender] + _mintAmount <= mintLimit_WL, "Address acquired max amount of WL NFT");
        require(WL_TokenSold + _mintAmount <= WL_TokenQty, "Purchase would exceed WL supply");
        require(msg.value >= PRICE_WL * _mintAmount, "Insufficient funds");
        WL_Claimed[msg.sender] += _mintAmount;
        _safeMint(msg.sender, _mintAmount);
        WL_TokenSold += _mintAmount;
    }

    function mint_Public(uint256 _mintAmount) public payable {
        require(Mint_State == 3, "Public is not actived");
        require(_mintAmount <= maxMintPerTx, "Mint amount exceed max mint per transaction");
        require(totalSupply() + _mintAmount <= maxSupply, "Purchase would exceed max supply");
        require(msg.value >= PRICE_Public * _mintAmount, "Insufficient funds");
        public_Claimed[msg.sender] += _mintAmount;
        _safeMint(msg.sender, _mintAmount);
    }


    //Utility function
    function set_OGPrice(uint256 newPrice) external onlyOwner {
        PRICE_OG = newPrice;
    }
    function set_WLPrice(uint256 newPrice) external onlyOwner {
        PRICE_WL = newPrice;
    }
    function set_PublicPrice(uint256 newPrice) external onlyOwner {
        PRICE_Public = newPrice;
    }

    function flip_MintState(uint256 newMintState) external onlyOwner {
        Mint_State = newMintState;
    }

    function withdraw() external onlyOwner{
        contractBalance = address(this).balance;

        community.call{value: contractBalance / 100 * 20}("");
        investor_1.call{value: contractBalance / 100 * 10}("");
        investor_2.call{value: contractBalance / 100 * 10}("");
        investor_3.call{value: contractBalance / 1000 * 75}("");
        manager.call{value: contractBalance / 100 * 6}("");
        marketing_director.call{value: contractBalance / 100 * 5}("");
        dev_team.call{value: address(this).balance}("");
    }

    function setRoot1(bytes32 _root) external onlyOwner {
        merkleRoot1 = _root;
    }

    function setRoot2(bytes32 _root) external onlyOwner {
        merkleRoot2 = _root;
    }
     
    function isValid1(bytes32[] memory proof) public view returns (bool) {
      bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
      return MerkleProof.verify(proof, merkleRoot1, leaf);
    }

    function isValid2(bytes32[] memory proof) public view returns (bool) {
      bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
      return MerkleProof.verify(proof, merkleRoot2, leaf);
    }

    ////
    //URI management part
    ////   
    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory newBaseURI) public onlyOwner {
            baseURI = newBaseURI;
    }

    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory _tokenURI = _baseURI();
        return bytes(_tokenURI).length > 0 ? string(abi.encodePacked(_tokenURI, (_tokenId+1).toString(), uriSuffix)) : "";
    }  
}