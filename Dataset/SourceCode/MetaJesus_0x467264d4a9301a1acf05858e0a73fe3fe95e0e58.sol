pragma solidity ^0.8.10;




contract MetaJesusCore is ERC721, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter internal tokenIdCounter;

    string[13] legendaries = ["Dad_is_Dead", "Comandante", "Dank_Farrik", "Full_Metal_Messiah", "Somebody_Stop_Me", "Dejavu", "Like_Father_Like_Son", "Remember", "Stoned_Bender", "OG", "RIP", "Meta_Dude", "Meth_Jesus"];

    mapping(address => bool) public whiteList;
    mapping(address => bool) blackList;

    uint[] public goldenNecklaceTokens;

    mapping(uint => string) internal legendaryTokenName;
    
    struct Token {
        string background;
        string cross;
        string jesus;
        bool goldenNecklace;
        bool isLegendary;
        string legendaryName;
    }

    constructor() ERC721("Meta-Jesus", "MJ")
    {}

    event isMinted(uint tokenId, address to);
    event firstRevealed(uint tokenId);

    uint private revealIdFlag = 0;

    mapping(uint => Token) tokens;
    string baseURI;
    string legendaryBaseUri = "https://meta-jesus.mypinata.cloud/ipfs/QmdSi1fFf46wj4fVSH2GJA1NJWBEA5DMuWVEsWGaTzPJrk/";
    
    
    address constant LIGHT_BRINGER = 0x829442225Bae622B3c0a6Cf61a4C5051Da895092;
    address constant DIVINE_PAINTER = 0x3C921311B682bD9500B5b18c42D0235bE9Dd56b4;
    address constant WITNESS = 0x61Ae4d275327DBB067C7fA1Fbd49C04C6152cB8E;
    address constant MIND_WANDERER = 0xc2005a648c09D9660215C80aefC27171E52f077c;

    address constant TECHNICAL_LEAD = 0xa0c0632ae278f567Bec8df4032c7E11258a88549;
    address constant CONTRACT_DEVELOPER = 0x3F65BF8d10519e853B9497A8E72780314730242B;
    address constant BACKEND_DEVELOPER_1 = 0x8BcEdB932510b6dc3BB446898931F12B9a7e9C64;
    address constant BACKEND_DEVELOPER_2 = 0xdBF1a78E4986db00B54E0a424861264b635DcB4a;
    address constant ANALYST_TESTER = 0x26546801B1EEf90C602E84d7Eb2b7113Cd11E941;

    address public constant GOLDEN_ROSARY_WALLET = 0x76a88B551301A3e7Bf895F0686Cd96100c49e7F3;

    uint public constant MAX_SUPPLY = 3333;
    uint public constant tokenPrice = 0.0666 ether;
    uint public constant preSaleTokenPrice = 0.0333 ether;

    bool public freezed = false;

    uint constant maxJesusPurchase = 5;
    
    bool public isGoldenNecklaceDist = false;

    function distributeLegendaries() internal onlyOwner {
        uint index = 0;

        while(index < 13) {
            uint random = getRandomNumber(index + 1) % MAX_SUPPLY;
            if(bytes(legendaryTokenName[random]).length == 0) {
                legendaryTokenName[random] = legendaries[index];
                index++;
            }
        }
    }
    
    function firstReveal(uint revealCount) public onlyOwner {  
        uint loopCount;
        if(revealIdFlag + revealCount > tokenIdCounter.current())
            loopCount = tokenIdCounter.current(); 
        else
            loopCount = revealIdFlag + revealCount;
        

        for(uint i = revealIdFlag; i < loopCount; i++) {
            if(bytes(legendaryTokenName[revealIdFlag]).length != 0) {
                continue;
            }
            Token storage _token = tokens[i];

            uint random = getRandomNumber(i);

            random = getRandomNumber(random);
            revealBackground(_token, random);

            random = getRandomNumber(random);
            revealCross(_token, random);

            random = getRandomNumber(random);
            revealJesus(_token, random);

            emit firstRevealed(i);
        }

        if(loopCount == tokenIdCounter.current())
            revealIdFlag = tokenIdCounter.current();
        else
            revealIdFlag += revealCount;
        
    } 

    function revealBackground (Token storage _token, uint random) internal {
        if(random % 100 < 5)
                _token.background = "BGS";
            else if(random % 100 < 10)
                _token.background = "BGC";
            else if(random % 100 < 55)
                _token.background = "BGH";
            else
                _token.background = "BGW";

    }

    function revealCross(Token storage _token, uint random) internal {

            if(compareStrings(_token.background, "BGS"))
                _token.cross = "Color Cross";
            else {
                random = getRandomNumber(random);
                if(random % 100 < 5)
                    _token.cross = "Gold Cross";
                else if (random % 100 < 25)
                    _token.cross = "Black Cross";
                else
                    _token.cross = "Wood Cross";

            }
    }

    function revealJesus(Token storage _token, uint random) internal {
        random = getRandomNumber(random);

            if(compareStrings(_token.background, "BGS")) {
                if(random % 100 < 5)
                    _token.jesus = "Hot Anim Black";
                else if(random % 100 < 15)
                    _token.jesus = "Hot Still Black";
                else if(random % 100 < 30)
                    _token.jesus = "Psyc Anim";
                else
                    _token.jesus = "Psyc Still";
            } else {
                if(random % 100 < 5)
                    _token.jesus = "Psyc";
                else if(random % 100 < 10)
                    _token.jesus = "Black";
                else
                    _token.jesus = "White";
            }

    }

    function getRandomNumber(uint id) internal view returns(uint) {
        
        return uint(keccak256(abi.encode(id,
            msg.sender,
            block.number,
            block.timestamp,
            blockhash(block.number - 1))));            
    }    
    
    function addWhitelist(address[] memory addrs) onlyOwner public {
        for(uint i = 0; i < addrs.length; i++) {
            require(!whiteList[addrs[i]], "Already whitelisted");
            whiteList[addrs[i]] = true;
        }
    }

    function addToBlacklist(address[] memory _addresses) onlyOwner public {
        for (uint i = 0; i < _addresses.length; i++) {
            require(!blackList[_addresses[i]], "Already blacklisted");
            blackList[_addresses[i]] = true;
        }
    }

    function getToken(uint tokenId) onlyOwner public view returns(Token memory){
        require(_exists(tokenId), "token does not exist");
        return tokens[tokenId];
    }

    function getTokenWithGoldenNecklace() public view returns(uint[] memory) {
        return goldenNecklaceTokens;        
    }

    function currentSupply() public view returns (uint) {
        return tokenIdCounter.current();
    }

    function compareStrings(string memory a, string memory b) internal pure returns (bool) {
        return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
    }

}
