  /**
   * @dev Sets `_tokenURI` as the tokenURI of `tokenId`.
   * @param tokenId - the tokenId to set the URI for
   * @notice Only the owner of the contract can set its URI.
   */
  function _setTokenURI(uint256 tokenId) internal virtual {
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    require(_exists(tokenId), "LibDiamondDapes: URI set of nonexistent token");

    bytes32 baseURIHash = keccak256(bytes(dds.baseURI));
    bytes32 emptyStringHash = keccak256(bytes(""));
    if (baseURIHash == emptyStringHash) {
      LibDiamondDapes.adminStorage()._tokenURIs[tokenId] = string(
        abi.encodePacked(dds.uriPrefix, _toString(tokenId), dds.uriSuffix)
      );
    } else {
      LibDiamondDapes.adminStorage()._tokenURIs[tokenId] = string(
        abi.encodePacked(dds.baseURI, _toString(tokenId), dds.uriSuffix)
      );
    }
  }

  function _updateRevealedStatus(uint256[] memory tokenIds, uint256 count) internal {
    LibDiamondDapes.AdminStorage storage adminStore = LibDiamondDapes.adminStorage();
    mapping(uint256 => bool) storage revealedTokens = adminStore.revealedTokens;

    for (uint256 i = 0; i < count; i++) {
      uint256 tokenId = tokenIds[i];
      if (!revealedTokens[tokenId]) {
        revealedTokens[tokenId] = true; // Only update the status if it was not revealed before
      }
    }
  }

  /**
   * @dev Reveals the tokenURI of `tokenId`.
   * @param _tokenIds - the tokenId to reveal
   * @notice Only the owner of the contract can reveal its URI.
   */
  function revealIndividualTokens(uint256[] calldata _tokenIds) external onlyOwner {
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    uint256 numTokens = _tokenIds.length;
    require(numTokens > 0, "NoTokensToReveal");

    uint256[] memory unrevealedTokens = new uint256[](numTokens);
    uint256 unrevealedCount = 0;

    for (uint256 i = 0; i < numTokens; i++) {
      uint256 _tokenId = _tokenIds[i];
      if (!LibDiamondDapes.adminStorage().revealedTokens[_tokenId]) {
        unrevealedTokens[unrevealedCount] = _tokenId;
        unrevealedCount++;
      }
    }

    if (unrevealedCount > 0) {
      _batchSetTokenURI(unrevealedTokens, unrevealedCount);
      dds.totalRevealed += unrevealedCount; // Update the total revealed count once after the batch processing
      _updateRevealedStatus(unrevealedTokens, unrevealedCount); // Update the revealed status for the processed tokens
    }
  }

  /**
   * @dev Returns the tokenURI of `tokenId`.
   * @param _tokenId - the tokenId to return
   */
  function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    LibDiamondDapes.AdminStorage storage aStore = LibDiamondDapes.adminStorage();

    if (!aStore.revealedTokens[_tokenId] && !_exists(_tokenId)) {
      revert("URIQueryForNonexistentToken");
    }

    if (!dds.revealed) {
      if (aStore.revealedTokens[_tokenId]) {
        return aStore._tokenURIs[_tokenId];
      } else {
        return dds.hiddenMetadataUri;
      }
    } else if (aStore.revealedTokens[_tokenId]) {
      return aStore._tokenURIs[_tokenId];
    } else if (bytes(aStore._tokenURIs[_tokenId]).length == 0) {
      string memory fullURI;
      if (bytes(dds.baseURI).length > 0) {
        fullURI = string(abi.encodePacked(dds.baseURI, _toString(_tokenId), dds.uriSuffix));
      } else {
        fullURI = string(abi.encodePacked(dds.uriPrefix, _toString(_tokenId), dds.uriSuffix));
      }
      return fullURI;
    } else {
      return aStore._tokenURIs[_tokenId];
    }
  }

  /**
   * @dev Sets the individual tokenURI of `tokenId`.
   * @param _tokenId - the tokenId to set the URI for
   * @param _setURI - the URI to set
   */
  function setIndividualTokenURI(uint256 _tokenId, string memory _setURI) external onlyOwner {
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    LibDiamondDapes.AdminStorage storage aStore = LibDiamondDapes.adminStorage();

    // Ensure that metadata is not frozen
    require(dds.metadataFrozen == false, "ApeFathers: metadata is frozen");

    // Check if the token exists
    require(_exists(_tokenId), "ApeFathers: token does not exist");

    string memory fullURI;
    if (bytes(_setURI).length > 0) {
      fullURI = string(abi.encodePacked(_setURI, _toString(_tokenId), dds.uriSuffix));
    } else if (bytes(dds.baseURI).length > 0) {
      fullURI = string(abi.encodePacked(dds.baseURI, _toString(_tokenId), dds.uriSuffix));
    } else {
      fullURI = string(abi.encodePacked(dds.uriPrefix, _toString(_tokenId), dds.uriSuffix));
    }
    aStore._tokenURIs[_tokenId] = fullURI;
  }

  /**
   * @dev Function to reveal tokens in batches.
   * @notice Can only be called by the contract owner.
   */
  function revealTokens(uint256 batchSize) external onlyOwner {
    LibDiamondDapes.DiamondDapesStruct storage dds = LibDiamondDapes.diamondDapesStorage();
    uint256 tokensToReveal;
    if (batchSize < 5) {
      tokensToReveal = min(dds.tokensPerBatch, this.totalSupply().sub(dds.totalRevealed + 1));
    } else {
      tokensToReveal = min(batchSize, this.totalSupply().sub(dds.totalRevealed + 1));
    }
    require(tokensToReveal > 0, "MoreThanZERO");
    uint256[] memory unrevealedTokens = new uint256[](tokensToReveal);
    uint256 unrevealedCount = 0;

    // Collect unrevealed tokens starting from the last revealed token
    uint256 startTokenId = dds.totalRevealed;
    uint256 endTokenId = startTokenId.add(tokensToReveal).sub(1);
    if (endTokenId >= LibDiamondDapes.MAX_SUPPLY) {
      endTokenId = LibDiamondDapes.MAX_SUPPLY;
      dds.revealed = true; // All tokens have been revealed
      dds.collectionRevealed = true; // All tokens have been revealed
    }

    for (uint256 tokenId = startTokenId; tokenId <= endTokenId; tokenId++) {
      if (LibDiamondDapes.adminStorage().revealedTokens[tokenId]) {
        continue; // Skip already revealed tokens
      }
      unrevealedTokens[unrevealedCount] = tokenId;
      unrevealedCount++;
    }

    if (unrevealedCount > 0) {
      _batchSetTokenURI(unrevealedTokens, unrevealedCount);
      dds.totalRevealed += unrevealedCount;
    }
  }

  /**
   * @dev Internal function to batch update the tokenURIs for multiple tokens.
   * @param tokenIds The array of token IDs to update the tokenURI for.
   * @param count The number of tokens to update.
   */
  function _batchSetTokenURI(uint256[] memory tokenIds, uint256 count) internal {
    LibDiamondDapes.AdminStorage storage adminStore = LibDiamondDapes.adminStorage();
    mapping(uint256 => bool) storage revealedTokens = adminStore.revealedTokens;

    for (uint256 i = 0; i < count; i++) {
      uint256 tokenId = tokenIds[i];
      if (!revealedTokens[tokenId]) {
        _setTokenURI(tokenId);
        revealedTokens[tokenId] = true; // Mark the token as revealed
      }
    }
  }

  /**
   * @dev Returns the smaller of two numbers.
   * @param a The first number.
   * @param b The second number.
   * @return The smaller of the two numbers.
   */
  function min(uint256 a, uint256 b) internal pure returns (uint256) {
    return a < b ? a : b;
  }

  /**
