contract DreamBeatzGenesis_Staking is Ownable{
    
    mapping(uint => uint) public rewardMultiplier; //24hours
    function setRewardMultiplier(uint level, uint reward_in_wei) public onlyOwner{
        rewardMultiplier[level] = reward_in_wei;
    }
    
    mapping(uint => bytes32) public merkleRoot;
    function setMerkleRoot(uint level, bytes32 m) public onlyOwner{
        merkleRoot[level] = m;
    }
    
    address public beatzAddress = 0xa5f365d79273BbAdC67F362ca359faD70d3D7391;
    function setBeatzAddress(address a) public onlyOwner{
        beatzAddress = a;
    }
    
    address public beatz_vault = 0x410DA9946c248Bd34A339d8CfB29e5cBDaE27dF0;
    function setBeatzVault(address a) public onlyOwner{
        beatz_vault = a;
    }
    
    address public beatzNFT = 0xBB15002647DE0E6a86779F2C9919c51fDb603083;
    function setBeatzNFT(address a) public onlyOwner{
        beatzNFT = a;
    }

    uint public period = 75600;
    function setPeriod(uint p) public onlyOwner{
        period = p;
    }

    struct Stake {
        uint id;
        address address_;
        uint time;
        uint level;
        bool staked;
    }
    
    mapping(uint => Stake) public STAKES;
    mapping(address => uint[]) public stakeIds;

////////////////////////////////////////////////////////////////////////////////////////

    function stake(uint256 id, uint level, bytes32[] calldata merkleproof) public {
        string memory indexNum = Strings.toString(id);
        require(MerkleProof.verify( merkleproof, merkleRoot[level], keccak256(abi.encodePacked(indexNum))), "Invalid level for Id!!");

        require(IERC721(beatzNFT).ownerOf(id) == msg.sender, "Invalid! id not found in user wallet!");
        require(STAKES[id].staked == false, "ID is alrady Staked!");
        IERC721(beatzNFT).transferFrom(msg.sender, beatz_vault, id);

        STAKES[id].id = id;
        STAKES[id].address_ = msg.sender;
        STAKES[id].time = block.timestamp;
        STAKES[id].level = level;
        STAKES[id].staked = true;

        stakeIds[msg.sender].push(id);
    }

    function claim(uint id) public {
        require(STAKES[id].address_ == msg.sender, "Invalid! address does not match user!");
        require(STAKES[id].staked == true, "ID is not Staked!");

        uint mx = rewardMultiplier[STAKES[id].level];
        uint reward = mx/period*(block.timestamp - STAKES[id].time);
        
        IERC20(beatzAddress).stakeReward(msg.sender, reward);
        STAKES[id].time = block.timestamp;
    }

    function claim_and_unstake(uint id) public {
        require(STAKES[id].address_ == msg.sender, "Invalid! address does not match user!");
        require(STAKES[id].staked == true, "ID is not Staked!");

        uint mx = rewardMultiplier[STAKES[id].level];
        uint reward = mx/period*(block.timestamp - STAKES[id].time);
        
        IERC20(beatzAddress).stakeReward(msg.sender, reward);
        STAKES[id].staked = false;
        IERC721(beatzNFT).transferFrom(beatz_vault, msg.sender, id);

        for(uint i=0 ; i<stakeIds[msg.sender].length; i++){
            if(stakeIds[msg.sender][i] == id) 
                delete stakeIds[msg.sender][i];
        }
    }

