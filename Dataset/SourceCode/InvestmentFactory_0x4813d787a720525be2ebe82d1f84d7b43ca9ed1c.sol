pragma solidity 0.8.4;









interface IOwnable {

        function owner() external view returns (address);

    }



interface IInvestment {

    function getContractTotalBalance() external view returns (uint256);

}





contract InvestmentFactory is Ownable {



    Investment investment;



    address public _feeHandlerAddress;



    event poolCreated(

        address poolCreator,

        address poolAddress,

        string poolName,

        string poolRisk,

        uint256 poolFee

    );

    

    constructor(address feeHandlerAddress_){

        _feeHandlerAddress = feeHandlerAddress_;

    }



    function changeFeeHandlerAddress(address _address) public onlyOwner {

        _feeHandlerAddress = _address;

    }



    function createInvestment(string memory name_, string memory symbol_, string memory risk_, uint256 poolFee_, uint256 transactionFee_) external {

        Investment newInvestment = new Investment(

            name_,

            symbol_,

            risk_,

            poolFee_,

            transactionFee_,

            address(this)

        );

        newInvestment.transferOwnership(msg.sender);

        emit poolCreated(msg.sender, address(newInvestment), name_, risk_, poolFee_);

    }





}