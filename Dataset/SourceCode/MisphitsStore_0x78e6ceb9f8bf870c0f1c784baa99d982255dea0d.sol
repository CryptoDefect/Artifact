    require(amount <=_whitelistIds[whitelistID].maxMint, "Misphits Store: Max amount exceded");
    require(!_walletMintedOn[whitelistID][msg.sender], "Misphits Store: Wallet already minted");
    require(_whitelistIds[whitelistID].minted.add(amount) <= _whitelistIds[whitelistID].maxSupply, "Misphits Store: Whitelist supply exceded");
    _walletMintedOn[whitelistID][msg.sender] = true;
    _whitelistIds[whitelistID].minted += uint16(amount);
    _misphits.mint(msg.sender, amount);
  }

  function mintTo(address to, bytes4 data ) external whenNotPaused nonReentrant onlyRole(BUYER_ROLE) {
    uint8 whitelistID = uint8(data[0]);
    require(block.timestamp < _whitelistIds[whitelistID].endsAt, "Misphits Store: Whitelist ended");
    uint8 amount = uint8(data[1]) + uint8(data[2]) + uint8(data[3]);
    require(amount <=_whitelistIds[whitelistID].maxMint, "Misphits Store: Max amount exceded");
    require(!_walletMintedOn[whitelistID][to], "Misphits Store: Wallet already minted");
    require(_whitelistIds[whitelistID].minted.add(amount) <= _whitelistIds[whitelistID].maxSupply, "Misphits Store: Whitelist supply exceded");
    _walletMintedOn[whitelistID][to] = true;
    _whitelistIds[whitelistID].minted += uint16(amount);
    _misphits.mint(to, amount);
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
  }

  function createWhitelist (uint8 id, uint8 maxMint, uint16 maxSupply, uint32 endsAt, uint256 price) external onlyRole(DEFAULT_ADMIN_ROLE) {
    _whitelistIds[id] = Whitelist(maxMint, maxSupply, 0, endsAt, price);
  }

  function getWhitelist (uint8 id) external view returns (uint8, uint16, uint16, uint32, uint256) {
    return (_whitelistIds[id].maxMint, _whitelistIds[id].maxSupply, _whitelistIds[id].minted, _whitelistIds[id].endsAt, _whitelistIds[id].price);
  }

  function walletMinted (uint8 whitelist, address wallet) external view returns (bool) {
    return  _walletMintedOn[whitelist][wallet];
  }

  function withdraw() external onlyOwner {
    payable(owner()).transfer(address(this).balance);
  }

  function setSigner(address signer_) external onlyRole(DEFAULT_ADMIN_ROLE) {
    _signer = signer_;
  }

  function setMisphits(address misphits_) external onlyRole(DEFAULT_ADMIN_ROLE) {
    _misphits = IMisphits(misphits_);
  }

  function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
    _pause();
  }

  function unPause() external onlyRole(DEFAULT_ADMIN_ROLE) {
    _unpause();
  }
}