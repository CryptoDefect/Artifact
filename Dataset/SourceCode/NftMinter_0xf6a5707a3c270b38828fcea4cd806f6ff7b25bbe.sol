contract NftMinter is Ownable {
    using SafeERC20 for IERC20;
    
    uint256 public current_supply;
    uint256 public current_sold;
    IReferBook public referBook;
    uint8 public defaultRate;
    bool public sendCommission;
    uint public price;
    uint public buy_limit_per_address = 10;
    uint public sell_begin_time = 0;
    mapping (address => uint) public mintRecord;
    mapping (address => uint8) private _rates;
    mapping (address => bool) private _sendCommissions;
    ISupportMint public nft;
    bytes32 public merkleRoot;
    
    event Minted(address indexed owner, address indexed reffer, uint count, uint payment);

    constructor(address nft_) {
        nft = ISupportMint(nft_);
    }

    function rateX1000(address sells) public view returns(uint8) {
        address sender = msg.sender;
        require(sender == sells || sender == owner());
        return _rateImpl(sells);
    }

    function referrer(address referal) public view returns (address) {
        if (address(referBook) != address(0)) {
            return referBook.getRefer(referal);
        } else {
            return address(0);
        }
    }

    function openMintAndPricing(uint256 supply, uint256 _price, uint256 _limit, uint256 _time, bytes32 root) public onlyOwner {
        current_supply = supply;
        price = _price;
        buy_limit_per_address = _limit;
        sell_begin_time = _time;
        merkleRoot = root;
    }

    function setReferBook(IReferBook value) public onlyOwner {
        require(address(value) != address(0), "PARAM_ERROR");
        referBook = value;
    }

    function setRateX1000(uint8 defaultValue, address[] memory sells, uint8[] memory _sellRates) public onlyOwner {
        require(sells.length == _sellRates.length, "PARAM_ERROR");
        for (uint i = 0; i < sells.length; ++i) {
            _rates[sells[i]] = _sellRates[i];
        }

        defaultRate = defaultValue;
    }

    function setSendCommission(bool defaultValue, address[] memory sells, bool[] memory sends) public onlyOwner {
        require(sells.length == sends.length, "PARAM_ERROR");
        for (uint i = 0; i < sells.length; ++i) {
            _sendCommissions[sells[i]] = sends[i];
        }

        sendCommission = defaultValue;
    }

    function withdraw(address token) public onlyOwner {
        if (token == address(0)) {
            uint balance = address(this).balance;
            Address.sendValue(payable(owner()), balance);
        } else {
            IERC20 erc20 = IERC20(token);
            erc20.safeTransfer(owner(), erc20.balanceOf(address(this)));
        }
    }

    function buy(uint amount, address to, string memory hash, address referrer_, bytes32[] calldata _merkleProof) public payable {
        if (merkleRoot != 0) {
            bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
            require(
                MerkleProof.verify(_merkleProof, merkleRoot, leaf),
                "Invalid proof."
            );
        }
        require(block.timestamp >= sell_begin_time, "Sell_Not_Start_Yet");

        mintRecord[msg.sender] = SafeMath.add(mintRecord[msg.sender], amount);
        require(mintRecord[msg.sender] <= buy_limit_per_address, "Exceed_Purchase_Limit");
        
        uint requiredValue = SafeMath.mul(amount, price);
        require(msg.value >= requiredValue, "Not_Enough_Payment");

        refer(referrer_);
        batchMintImpl(to, hash, amount);

        referrer_ = referrer(msg.sender);
        bool sc = _sendCommissions[referrer_];
        if (!sc) {
            sc = sendCommission;
        }
        if (sc && referrer_ != address(0)) {
            uint commission = _rateImpl(referrer_);
            commission = SafeMath.mul(requiredValue, commission) / 1000;
            Address.sendValue(payable(referrer_), commission);
        }

        emit Minted(msg.sender, referrer_, amount, requiredValue);
    }

    function batchMintImpl(address to, string memory hash, uint amount) private {
        current_sold = SafeMath.add(current_sold, amount);
        require(current_supply >= current_sold, "Not_Enough_Stock");

        for (uint i = 0; i < amount; ++i) {
            nft.mint(to, hash, to);
        }
    }

    function _rateImpl(address sells) private view returns (uint8) {
        uint8 result = _rates[sells];
        if (result == 0) {
            result = defaultRate;
        }

        return result;
    }

    function refer(address referrer_) private {
        require(!Address.isContract(referrer_), "human only!");
        if (address(referBook) != address(0)) {
            referBook.addReferForNoExist(_msgSender(), referrer_);
        }
    }
}