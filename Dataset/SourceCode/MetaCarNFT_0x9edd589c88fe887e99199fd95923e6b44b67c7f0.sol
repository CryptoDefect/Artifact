// File contracts/MetaCarNFT.sol


pragma solidity ^0.8.0;
contract MetaCarNFT is
    ERC721,
    ERC721Enumerable,
    ERC721URIStorage,
    ERC721Holder,
    Ownable,
    ReentrancyGuard,
    PullPayment,
    AccessControl
{
    using Counters for Counters.Counter;

    struct Creator {
        uint96 royality;
        address creator;
    }

    struct OrderBook {
        uint64 tokenId;
        address currency;
        uint128 price;
        uint128 reservedPrice;
        uint80 startTime;
        uint80 endTime;
        bool isClosed;
        bool isAuction;
        address bidder;
    }


    IERC20 private _token;
    uint256 public auctionEndTime = (8 * 24 * 60 * 60);

    mapping(uint256 => Creator) public creatorOf;
    mapping(uint256 => OrderBook) public orderBook;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    address payable private _contractOwner;
    uint96 public fee = 5;
    uint96 public maxRoyalty = 5;

    string baseURI;

    uint256 public mintPrice = 0.1 ether;
    uint128 constant MAX_TOKENS = 5040;
    uint128 constant MAX_MINTAMOUNT = 20;

    
    uint256 vrfSeed;

    uint256 private nonce = 0;
    uint256 private before = 0;

    event MintToken(
        address indexed owner,
        uint256 tokenId,
        uint256 timestamp
    );
    event SellToken(
        address indexed seller,
        uint256 indexed tokenId,
        uint256 price,
        address currency,
        bool auction,
        uint256 endTime,
        uint256 timestamp
    );
    event BuyToken(
        address indexed seller,
        address indexed buyer,
        uint256 indexed tokenId,
        address currency,
        uint256 price,
        uint256 timestamp
    );
    event Bid(uint256 indexed tokenId, address indexed bidder, uint256 price);
    event PriceUpdated(uint256 indexed tokenId, uint256 price);
    event WithdrawnSellOrder(uint256 indexed tokenId);

    modifier checkRoyality(uint32 royality) {
        require(
            (royality >= 0) && (royality <= 100) && (royality <= maxRoyalty),
            "Royality can not be less than maxRoyalty nor 0 nor more than 100."
        );
        _;
    }

    modifier isOrderClosed(uint256 orderId) {
        require(
            orderBook[orderId].isClosed != true,
            "Order has been sold already"
        );
        _;
    }

    modifier _minter() {
        require(hasRole(MINTER_ROLE, msg.sender), "User cannot mint");
        _;
    }

    constructor(uint256 _randomNumber,string memory _baseURI) ERC721("McQueen and his buddies", "MQB") {
        _contractOwner = payable(msg.sender);
        baseURI = _baseURI;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        vrfSeed = _randomNumber;
    }

    function tokensOfOwner(address _owner)
        external
        view
        returns (uint256[] memory)
    {
        uint256 tokenCount = balanceOf(_owner);
        if (tokenCount == 0) {
            // Return an empty array
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 index;
            for (index = 0; index < tokenCount; index++) {
                result[index] = tokenOfOwnerByIndex(_owner, index);
            }
            return result;
        }
    }


    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC721Enumerable, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _burn(uint256 tokenId)
        internal
        virtual
        override(ERC721, ERC721URIStorage)
    {
        super._burn(tokenId);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    receive() external payable {}

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory _tokenURI = string(
            abi.encodePacked(baseURI, "/", Strings.toString(tokenId), ".json")
        );

        return _tokenURI;
    }

    function random() internal  returns (uint256) {
        require(vrfSeed != 0, "Invalid seed");
        nonce++;
        uint256 random_ = uint256(keccak256(abi.encode(vrfSeed,nonce,before)));
        before =  (random_ & 0xFFFFF) % MAX_TOKENS;
        return before;
    }

    function newTokenId() internal  returns (uint256) {
        uint256 tokenId = random();
        bool minted = _exists(tokenId);

        while (minted) {
            tokenId = tokenId = random();
            minted = _exists(tokenId);
        }
        return tokenId;
    }

    function mint(uint256 _amount) external  payable nonReentrant{
        require(_amount <= MAX_MINTAMOUNT, "not mint more than max amount");
        
        uint256 price = mintPrice * _amount; 
       
        require(msg.value >= price, "not enough funds");

        for(uint256 i = 0; i < _amount; i++){
        
            require(totalSupply() < MAX_TOKENS, "sold out!");

            uint256 random_ = newTokenId();

            _mintToken(msg.sender, random_,  maxRoyalty);
        }

    }

    function mintToken(uint32 royality)
        public
        _minter
        checkRoyality(royality)
    {
        require(totalSupply() < MAX_TOKENS, "sold out!");
        uint256 random_ = newTokenId();
        _mintToken(msg.sender, random_,  royality);
    }


    function _mintToken(
        address _user,
        uint256 _tokenId,
        uint96 _royality
    ) private {
        _safeMint(_user, _tokenId);

        creatorOf[_tokenId].royality = _royality;
        creatorOf[_tokenId].creator = _user;

        emit MintToken(_user, _tokenId, block.timestamp);
    }

    function sellToken(
        uint64 tokenId,
        uint128 price,
        address currency,
        uint256 startTime,
        uint256 endTime,
        uint256 reservedPrice,
        bool isAuction
    ) public {
        _onlyOwnerAndIfExists(tokenId, msg.sender);
        _sellToken(
            msg.sender,
            tokenId,
            price,
            currency,
            startTime,
            endTime,
            reservedPrice,
            isAuction
        );
    }

    function _sellToken(
        address _user,
        uint64 _tokenId,
        uint128 _price,
        address _currency,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _reservedPrice,
        bool _isAuction
    ) private {
        orderBook[_tokenId].tokenId = _tokenId;
        orderBook[_tokenId].price = _price;
        orderBook[_tokenId].currency = _currency;
        orderBook[_tokenId].startTime = _startTime > 0
            ? uint80(_startTime)
            : uint80(block.timestamp);

        if (_isAuction) {
            orderBook[_tokenId].isAuction = _isAuction;
            orderBook[_tokenId].endTime = _endTime > 0
                ? uint80(_endTime)
                : uint80(block.timestamp + auctionEndTime);
            orderBook[_tokenId].reservedPrice = uint128(_reservedPrice);
        }

        emit SellToken(
            _user,
            _tokenId,
            _price,
            _currency,
            _isAuction,
            orderBook[_tokenId].endTime,
            block.timestamp
        );
    }

    function withdrawSellOrder(uint64 tokenId) public {
        _onlyOwnerAndIfExists(tokenId, msg.sender);
        _invalidOrderId(tokenId);
        require(
            orderBook[tokenId].isAuction != true,
            "You are not allowed to withdraw auction orders."
        );
        delete orderBook[tokenId];
        emit WithdrawnSellOrder(tokenId);
    }

    function updateSellingPrice(uint64 tokenId, uint64 price) public {
        _onlyOwnerAndIfExists(tokenId, msg.sender);
        _invalidOrderId(tokenId);
        require(
            orderBook[tokenId].isAuction != true,
            "You are not allowed to update auction orders."
        );
        orderBook[tokenId].price = price;
        emit PriceUpdated(tokenId, price);
    }

    function buyToken(uint256 orderId, uint256 tokenValue)
        public
        payable
        isOrderClosed(orderId)
        nonReentrant
    {
        require(
            orderBook[orderId].isAuction != true,
            "You are not allowed to buy auction orders."
        );
        _invalidOrderId(orderId);
        _validateOwner(orderId);
        if (orderBook[orderId].currency == address(0)) {
            _buyToken(orderId, msg.sender, msg.value, true);
        } else {
            _buyToken(orderId, msg.sender, tokenValue, false);
        }
    }

    function _buyToken(
        uint256 orderId_,
        address _buyer,
        uint256 _value,
        bool isBuyingWithEth
    ) private {
        uint256 price = orderBook[orderId_].price;
        require(
            price == _value,
            "Value must be equal to price of selling order."
        );

        address seller = ownerOf(orderBook[orderId_].tokenId);
        uint256 royality = _calcRoyality(orderBook[orderId_].tokenId, price);
        uint256 platform = _calcFee(price);
        uint256 sellAmt = price - royality - platform;

        if (isBuyingWithEth) {
            payable(creatorOf[orderId_].creator).transfer(royality);
            payable(_contractOwner).transfer(platform);
            payable(seller).transfer(sellAmt);
        } else {
            _token = IERC20(orderBook[orderId_].currency);
            _token.transferFrom(_buyer, address(this), _value);
            _transferTokens(
                orderBook[orderId_].tokenId,
                seller,
                royality,
                sellAmt,
                platform
            );
        }

        _safeTransfer(seller, _buyer, orderBook[orderId_].tokenId, "");
        orderBook[orderId_].isClosed = true;
        emit BuyToken(
            seller,
            _buyer,
            orderBook[orderId_].tokenId,
            orderBook[orderId_].currency,
            _value,
            block.timestamp
        );
        delete orderBook[orderId_];
    }

    function bidOnAuction(uint256 orderId, uint256 value)
        public
        payable
        isOrderClosed(orderId)
        nonReentrant
    {
        _validateAuction(orderId);
        _invalidOrderId(orderId);
        _validateOwner(orderId);

        if (orderBook[orderId].currency == address(0)) {
            _bidOnAuction(orderId, msg.sender, msg.value);
        } else {
            _bidOnAuction(orderId, msg.sender, value);
        }
    }

    function _bidOnAuction(
        uint256 orderId_,
        address _bidder,
        uint256 _value
    ) private {
        OrderBook storage order = orderBook[orderId_];

        require(
            block.timestamp > order.startTime,
            "Auction is not started yet."
        );

        require(
            order.price < _value,
            "Value must be greator than last bid price."
        );

        _isTokenExists(order.tokenId);

        if (block.timestamp > order.endTime) {
            if (
                (block.timestamp > order.endTime) &&
                (order.bidder == address(0))
            ) {
                order.isAuction = false;
                order.price = order.reservedPrice;
            }

            revert("Auction has been expired already.");
        }

        if (order.currency == address(0)) {

            if (order.bidder != address(0) && order.price > 0) {
                _safeTransferETH(order.bidder,  order.price);
            }
            
            order.bidder = _bidder;
            order.price = uint128(msg.value);
        } else {

            require(IERC20(order.currency).transferFrom(_bidder, address(this), _value), "transfer payment token failed");
            
            if (order.bidder != address(0) && order.price > 0) {
                IERC20(order.currency).transfer(order.bidder, order.price);
            }

            order.bidder = _bidder;
            order.price = uint128(_value);
        }
        emit Bid(orderId_, _bidder, _value);
    }

    function claimAuction(uint256 orderId) public isOrderClosed(orderId) {
        _validateAuction(orderId);
        _invalidOrderId(orderId);
        _claimAuction(orderId);
    }

    function _claimAuction(uint256 _orderId) private {
        OrderBook memory order = getOrder(_orderId);
        address _seller = ownerOf(order.tokenId);
        uint256 _value = order.price;

        uint256 _royality = _calcRoyality(order.tokenId, _value);
        uint256 platform = _calcFee(_value);
        uint256 _sellAmt = _value - _royality - platform;

        if ((order.bidder == address(0))) {
            revert("No Bids placed.");
        }

        if (order.currency == address(0)) {
            payable(creatorOf[_orderId].creator).transfer(_royality);
            payable(_contractOwner).transfer(platform);
            payable(_seller).transfer(_sellAmt);
        } else {
            _token = IERC20(order.currency);
            _transferTokens(order.tokenId, _seller, _royality, _sellAmt, platform);
        }

        _safeTransfer(_seller, order.bidder, order.tokenId, "");

        orderBook[_orderId].isClosed = true;

        emit BuyToken(
            _seller,
            order.bidder,
            _orderId,
            order.currency,
            _value,
            block.timestamp
        );
        delete orderBook[_orderId];
    }


    function changeRoyality(uint256 tokenId, uint32 royality)
        public
        checkRoyality(royality)
    {
        require(
            creatorOf[tokenId].creator == msg.sender,
            "Only creator can change royality percentage."
        );
        creatorOf[tokenId].royality = royality;
    }

    function _calcRoyality(uint256 _tokenId, uint256 _price)
        private
        view
        returns (uint256)
    {
        uint256 royality_ = creatorOf[_tokenId].royality;
        return ((_price * royality_) / 100);
    }

    function _calcFee(uint256 _price) private view returns (uint256) {
        return ((_price * fee) / 100);
    }

    function _onlyOwnerAndIfExists(uint256 tokenId, address user) private view {
        _isTokenExists(tokenId);
        require(ownerOf(tokenId) == user, "Only owner can call.");
    }

    function _isTokenExists(uint256 tokenId) private view {
        require(_exists(tokenId), "Token id do not exist");
    }

    function _invalidOrderId(uint256 orderId) private view {
        require(orderBook[orderId].tokenId != uint256(0), "Invalid order ID");
    }

    function validateIsAuction(uint256 orderId) private view {
        require(
            orderBook[orderId].isAuction == true,
            "Order id do not specified as auction order"
        );
    }

    function _validateOwner(uint256 orderId) private view {
        address _owner = ownerOf(orderBook[orderId].tokenId);
        require(
            _owner != msg.sender,
            "seller can not perform action on this order."
        );
    }

    function _validateAuction(uint256 orderId) private view {
        require(
            orderBook[orderId].isAuction == true,
            "Order is not an auction order."
        );
    }


    function _safeTransferETH(address to, uint256 value) internal returns(bool) {
		(bool success, ) = to.call{value: value}(new bytes(0));
		return success;
    }

    function _transferTokens(
        uint256 _tokenId,
        address _seller,
        uint256 _royality,
        uint256 _sellAmt,
        uint256 _platform
    ) private {
        _token.transfer(creatorOf[_tokenId].creator, _royality);
        _token.transfer(_contractOwner, _platform);
        _token.transfer(_seller, _sellAmt);
    }

    function getOrder(uint256 _orderId) public view returns (OrderBook memory) {
        return orderBook[_orderId];
    }

    function updateOper(address newOper) external {
        require(msg.sender == _contractOwner, "Error, you are not the owner");
        _contractOwner = payable(newOper);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _asyncTransfer(msg.sender, address(this).balance);
    }

    function updateFee(uint96 newFee) external onlyOwner{
        fee = newFee;
    }

    function updateMaxRoyalty(uint96 newFee) external onlyOwner {
        maxRoyalty = newFee;
    }

    function setBaseURI(string memory _baseURI) external {
        require(msg.sender == _contractOwner, "Error, you are not the owner");
        baseURI = _baseURI;
    }

    function setChangeSeed(uint256 _vrfSeed) external  {
        require(msg.sender == _contractOwner, "Error, you are not the owner");
        vrfSeed = _vrfSeed;
    }

    function setMintPrice(uint256 _price) external onlyOwner {
        mintPrice = _price;
    }

    function recoverWrongTokens(address _tokenAddress, uint256 _tokenAmount) external onlyOwner {
        IERC20(_tokenAddress).transfer(address(msg.sender), _tokenAmount);
    }

    function withdrawETH(address recipient, uint256 amount) external onlyOwner {
        require(address(this).balance > 0, "Insufficient balance");
        (bool res,) = recipient.call{value : amount}("");
        require(res, "ETH TRANSFER FAILED");
    }
}