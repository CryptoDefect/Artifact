contract Chat2EarnProps is ERC721A, OwnerCheck {
    using SafeMath for uint256;
    using Strings for uint256;
    using SafeERC20 for IERC20;

    IERC20 public metaToken = IERC20(0x364fcd7325C035CC4F2cdE8b6c8D7Df5e7Db6589);
    address public signer = 0xB82Dda08bFcE8C51F1e37ebE9bc8D7d4Bc0547Ba;
    address payable public beneficiary = payable(0x2848562b3fabA0C03A24a0369ce19D6B69fEbBAb);
    uint16 public power = 1500;
    uint16 public maxSupply = 20000;

    struct LockData {
        uint128 cardId;
        uint32 timestamp; //lock time for lock,update to buy energy time when call buyenergy
        uint96 claimedMeta; //already claimed meta
    }
    //user address=>lock data
    mapping(address => LockData) public lockInfo;
    mapping(address => uint256) public nonce;

    event LockCard(address user, uint256 cardId);
    event BuyEnergy(address user, uint256 cardId, uint256 amount, bool useBalance, uint8 day);
    event Claim(address user, uint256 amount);

    constructor(string memory _name, string memory _symbol) ERC721A(_name, _symbol) {
        _setBaseURI("https://storage.googleapis.com/");
    }

    function lockCard(uint256 cardId) public lock {
        address sender = msg.sender;
        require(sender == ownerOf(cardId), "not owner");
        LockData storage ld = lockInfo[sender];
        require(ld.cardId == 0, "already lock");

        //transfer card to this contract
        safeTransferFrom(sender, address(this), cardId);

        ld.cardId = uint128(cardId);
        ld.timestamp = uint32(block.timestamp);
        emit LockCard(sender, cardId);
    }

    function buyEnergy(bytes32 r, bytes32 s, uint8 v, uint256 amount, uint8 day, uint256 total, uint32 non, bool useBalance) external lock {
        address sender = msg.sender;
        require(non == nonce[sender] + 1, "nonce error");
        nonce[sender] = non;
        LockData storage ld = lockInfo[sender];
        require(ld.cardId != 0, "lock first");
        require(signer == ecrecover(keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", keccak256(abi.encodePacked(sender, block.chainid, address(this), amount, day, total, non)))), v, r, s), "sign");
        if (useBalance) {
            require(lockInfo[sender].claimedMeta + amount <= total, "balance not enough");
            lockInfo[sender].claimedMeta += uint96(amount);
        } else {
            metaToken.safeTransferFrom(sender, beneficiary, amount);
        }
        ld.timestamp = uint32(block.timestamp);

        emit BuyEnergy(sender, ld.cardId, amount, useBalance, day);
    }

    function claim(bytes32 r, bytes32 s, uint8 v, uint256 amount, uint256 total) external lock {
        address sender = msg.sender;
        LockData storage ld = lockInfo[sender];
        require(ld.cardId != 0, "no card");
        require(uint256(ld.claimedMeta) + amount <= total, "invalid amount");
        require(signer == ecrecover(keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", keccak256(abi.encodePacked(sender, block.chainid, address(this), amount, total)))), v, r, s), "sign");

        metaToken.safeTransfer(sender, amount);
        ld.claimedMeta += uint96(amount);
        emit Claim(sender, amount);
    }

    function mint(address to, uint256 count) external onlyOperator {
        require(totalSupply() <= maxSupply, "over max supply");
        _mint(to, count, new bytes(0), false);
    }

    function setBaseTokenURI(string memory baseTokenURI) external onlyOperator {
        _setBaseURI(baseTokenURI);
    }

    function getBaseURI() external view returns (string memory){
        return baseURI();
    }

    function currentIndex() public view returns (uint256){
        return _currentIndex;
    }

    function setBeneficiary(address payable _beneficiary) public onlyOperator {
        beneficiary = _beneficiary;
    }

    function setMetaToken(IERC20 _metaToken) public onlyOperator {
        metaToken = _metaToken;
    }

    function setSigner(address _signer) public onlyOperator {
        signer = _signer;
    }

    function setPower(uint16 _power) public onlyOperator {
        power = _power;
    }

    function setMaxSupply(uint16 _maxSupply) public onlyOperator {
        maxSupply = _maxSupply;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public pure returns (bytes4) {
        return this.onERC721Received.selector;
    }
}