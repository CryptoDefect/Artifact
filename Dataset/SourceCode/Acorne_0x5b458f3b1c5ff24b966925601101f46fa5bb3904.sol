        for (uint256 i = 0; i < _amount; i++) {
            commissions[tokenId + i] = Commission({
                value: 0,
                lastClaimedAt: block.timestamp,
                lastClaimedAddress: _address
            });
            // Send message to child chain via portal
            _sendMessageToChild(
                abi.encode(MINTED, abi.encode(_address, tokenId))
            );
        }
    }

