    "contracts/COAMarketPlaceV1.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\nimport \"@openzeppelin/contracts/utils/Address.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\nimport \"@openzeppelin/contracts/utils/cryptography/ECDSA.sol\";\nimport \"@openzeppelin/contracts/token/ERC721/IERC721.sol\";\nimport \"@openzeppelin/contracts/token/ERC1155/IERC1155.sol\";\nimport \"contracts/libs/TypedData.sol\";\nimport \"contracts/interfaces/ICOA.sol\";\nimport \"contracts/COANFT.sol\";\nimport \"contracts/types/CertificateV1.sol\";\n\ncontract COANFTMarketPlaceV1 is Ownable {\n    using ECDSA for bytes32;\n    using Address for address;\n\n    struct Payment {\n        uint256 amount;\n        TypedData.Signature signature;\n    }\n\n    CertificateV1 public certificate;\n\n    mapping(bytes32 => bool) public payments;\n\n    constructor(address certificate_) {\n        certificate = CertificateV1(certificate_);\n    }\n\n    /**\n     * @notice update the certificate address\n     * @param certificate_ the new certificate address\n     */\n    function updateCertificate(address certificate_) external onlyOwner {\n        certificate = CertificateV1(certificate_);\n    }\n\n    /**\n     * @notice create a certificate and mint an NFT with payment\n     * @param to_ the address to mint the NFT to\n     * @param certificate_ the certificate data\n     * @param signature_ the signature of the certificate\n     * @param nftContract_ the NFT contract address\n     * @param contractFunctionData_ the function data to call on the NFT contract to mint the NFT\n     * @param payment_ the payment data (amount and signature)\n     */\n    function createCertificateAndMintNFTWithPayment(\n        address to_,\n        TypedData.CertificateV1 calldata certificate_,\n        TypedData.Signature calldata signature_,\n        address nftContract_,\n        bytes calldata contractFunctionData_,\n        Payment calldata payment_\n    ) external payable {\n        uint256 nftPrice = (msg.value - payment_.amount);\n        require(nftContract_.isContract(), \"NFT contract is not a contract\");\n        require(contractFunctionData_.length > 0, \"NFT contract function data is empty\");\n        _createCertificateWithSignature(certificate_, to_, signature_);\n        nftContract_.functionCallWithValue(contractFunctionData_, nftPrice);\n        bytes32 dataHash = keccak256(\n            abi.encode(\n                to_,\n                signature_.signatureRaw,\n                nftContract_,\n                contractFunctionData_,\n                payment_.amount,\n                payment_.signature.nonce\n            )\n        );\n        _processPayment(payment_, dataHash);\n    }\n\n    /**\n     * @notice create a certificate and charge a payment. Can be used to create a certificate for\n     * physical artwork, for NFT that was already minted, for the first element of a fungible NFT,\n     * etc.\n     * @param to_ address to create the certificate for\n     * @param certificate_ certificate data\n     * @param signature_ signature of the certificate\n     * @param payment_ payment data (amount and signature)\n     */\n    function createCertificateWithPayment(\n        address to_,\n        TypedData.CertificateV1 calldata certificate_,\n        TypedData.Signature calldata signature_,\n        Payment calldata payment_\n    ) external payable {\n        _createCertificateWithSignature(certificate_, to_, signature_);\n        bytes32 dataHash = keccak256(\n            abi.encode(to_, signature_.signatureRaw, payment_.amount, payment_.signature.nonce)\n        );\n        _processPayment(payment_, dataHash);\n    }\n\n    /**\n     * @notice clone a certificate and charge a payment. Can be used for fungible NFTs.\n     * @param to_ address to create the certificate for\n     * @param certificateId_ certificate id to clone\n     * @param payment_ payment data (amount and signature)\n     */\n    function cloneCertificateWithPayment(\n        address to_,\n        uint256 certificateId_,\n        Payment calldata payment_\n    ) external payable {\n        certificate.cloneCertificate(to_, certificateId_, 1);\n        bytes32 dataHash = keccak256(\n            abi.encode(to_, certificateId_, payment_.amount, payment_.signature.nonce)\n        );\n        _processPayment(payment_, dataHash);\n    }\n\n    /**\n     * @notice process a payment. The payment amount is sent to the owner of the contract.\n     */\n    function _processPayment(Payment calldata payment_, bytes32 dataHash_) internal {\n        address signer = dataHash_.toEthSignedMessageHash().recover(\n            payment_.signature.signatureRaw\n        );\n        require(signer == owner(), \"Invalid signature\");\n        require(!payments[dataHash_], \"Payment already processed\");\n        payments[dataHash_] = true;\n        require(address(this).balance >= payment_.amount, \"Insufficient funds\");\n        (bool success, ) = owner().call{value: address(this).balance}(\"\");\n        require(success, \"Transfer failed.\");\n    }\n\n    /**\n     * @notice create a certificate with a signature\n     */\n    function _createCertificateWithSignature(\n        TypedData.CertificateV1 calldata certificate_,\n        address to_,\n        TypedData.Signature calldata signature_\n    ) internal {\n        TypedData.CertificateV1[] memory certificates = new TypedData.CertificateV1[](1);\n        address[] memory to = new address[](1);\n        TypedData.Signature[] memory signature = new TypedData.Signature[](1);\n        certificates[0] = certificate_;\n        to[0] = to_;\n        signature[0] = signature_;\n        certificate.createCertificateWithSignature(certificates, to, signature);\n    }\n}\n"

    },

    "contracts/COANFT.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\nimport \"@openzeppelin/contracts/token/ERC1155/ERC1155.sol\";\nimport \"@openzeppelin/contracts/access/Ownable2Step.sol\";\nimport \"@openzeppelin/contracts/utils/Strings.sol\";\nimport \"contracts/libs/Errors.sol\";\nimport \"contracts/interfaces/ICOA.sol\";\nimport \"contracts/interfaces/ICOANFT.sol\";\n\ncontract COANFT is Ownable2Step, ERC1155, ICOANFT {\n    /// @dev mapping of token id to token type\n    mapping(uint256 => uint8) public tokenType;\n    /// @dev mapping to keep track of the types that are blocked for transfers\n    mapping(uint8 => bool) public transferBlockedTypes;\n    /// address of the coa contract\n    ICOA public coa;\n    /// current tokenID\n    uint256 public tokenID;\n\n    modifier onlyOwnerOrCOA() {\n        if (msg.sender != owner() && msg.sender != address(coa)) {\n            revert NotOwnerOrCOA();\n        }\n        _;\n    }\n\n    modifier tokenWasMinted(uint256 tokenID_) {\n        if (tokenID_ > tokenID) {\n            revert TokenNotCreatedYet(tokenID_);\n        }\n        _;\n    }\n\n    modifier tokenIsValid(uint256 tokenID_) {\n        if (tokenType[tokenID_] == 0) {\n            revert TokenDoesNotExist(tokenID_);\n        }\n        _;\n    }\n\n    constructor(address coa_) ERC1155(\"COA\") {\n        coa = ICOA(coa_);\n    }\n\n    /**\n     * @notice update the coa address\n     * @param coa_ the new coa address\n     */\n    function updateCOA(address coa_) public onlyOwner {\n        coa = ICOA(coa_);\n    }\n\n    /**\n     * @notice block transfers for a token type\n     * @param tokenType_ the token type\n     * @param isBlocked_ if the transfers are blocked\n     */\n    function blockTransferForType(uint8 tokenType_, bool isBlocked_) public onlyOwner {\n        transferBlockedTypes[tokenType_] = isBlocked_;\n    }\n\n    /**\n     * @notice mint a new token\n     * @param to_ the address to mint to\n     * @param tokenType_ the token type\n     * @param amount_ the amount to mint\n     */\n    function mint(\n        address to_,\n        uint256 amount_,\n        uint8 tokenType_\n    ) public onlyOwnerOrCOA returns (uint256) {\n        if (tokenType_ == 0) {\n            revert InvalidTokenType();\n        }\n        uint256 newTokenID = _newTokenId();\n        tokenType[newTokenID] = tokenType_;\n        _mint(to_, newTokenID, amount_, \"\");\n        return newTokenID;\n    }\n\n    /**\n     * @notice invalidates a token. The invalidated token will have a default metadata.\n     * @param tokenID_ the token id\n     */\n    function invalidate(uint256 tokenID_) public tokenIsValid(tokenID_) onlyOwnerOrCOA {\n        delete tokenType[tokenID_];\n        // updating the metadata to be a invalid token\n        emit URI(coa.createMetadata(tokenID_, 0), tokenID_);\n    }\n\n    function updateTokenURI(uint256 tokenID_) public tokenIsValid(tokenID_) onlyOwnerOrCOA {\n        emit URI(coa.createMetadata(tokenID_, tokenType[tokenID_]), tokenID_);\n    }\n\n    /**\n     * @notice burn a token\n     * @param from_ the address to burn from\n     * @param tokenID_ the token id\n     * @param amount_ the amount to burn\n     */\n    function burn(address from_, uint256 tokenID_, uint256 amount_) public onlyOwnerOrCOA {\n        _burn(from_, tokenID_, amount_);\n    }\n\n    /**\n     * @notice clone a token\n     * @param to_ the address to clone to\n     * @param tokenID_ the token id\n     * @param amount_ the amount to clone\n     */\n    function clone(\n        address to_,\n        uint256 tokenID_,\n        uint256 amount_\n    ) public onlyOwnerOrCOA tokenIsValid(tokenID_) {\n        _mint(to_, tokenID_, amount_, \"\");\n    }\n\n    /**\n     * @notice burn an invalid token. This will remove the token from the user's balance.\n     * @param tokenID_ the token id\n     */\n    function burnInvalid(uint256 tokenID_) public {\n        if (tokenType[tokenID_] != 0) {\n            revert TokenIsNotInvalid();\n        }\n        uint256 userBalance = balanceOf(msg.sender, tokenID_);\n        if (userBalance == 0) {\n            revert NotEnoughBalance(msg.sender, tokenID_);\n        }\n        _burn(msg.sender, tokenID_, userBalance);\n    }\n\n    /**\n     * @notice get the token type\n     * @param tokenID_ the token id\n     * @return the token type\n     */\n    function getType(uint256 tokenID_) public view tokenWasMinted(tokenID_) returns (uint8) {\n        return tokenType[tokenID_];\n    }\n\n    /**\n     * @notice get the token uri\n     * @param tokenID_ the token id\n     * @return the token uri\n     */\n    function uri(\n        uint256 tokenID_\n    ) public view virtual override tokenWasMinted(tokenID_) returns (string memory) {\n        return coa.createMetadata(tokenID_, tokenType[tokenID_]);\n    }\n\n    /**\n     * @dev get the next tokenID\n     * @return the next tokenID\n     */\n    function _newTokenId() internal returns (uint256) {\n        // increment the tokenID\n        tokenID++;\n        // return the tokenID\n        return tokenID;\n    }\n\n    /**\n     * @dev before token transfer hook to prevent transferring identity tokens\n     */\n    function _beforeTokenTransfer(\n        address /*operator*/,\n        address from_,\n        address to,\n        uint256[] memory ids,\n        uint256[] memory /*amounts*/,\n        bytes memory /*data*/\n    ) internal virtual override {\n        for (uint256 i = 0; i < ids.length; i++) {\n            if (from_ == address(0) || to == address(0)) {\n                continue;\n            }\n            if (transferBlockedTypes[tokenType[ids[i]]]) {\n                revert CannotTransferIdentity(ids[i]);\n            }\n        }\n    }\n}\n"

    },

    "contracts/interfaces/ICOA.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\nimport \"contracts/libs/TypedData.sol\";\nimport \"@openzeppelin/contracts/access/IAccessControl.sol\";\n\ninterface ICOA is IAccessControl {\n    function mintToken(address to_, uint256 amount_) external returns (uint256);\n\n    function cloneToken(address to_, uint256 amount_, uint256 tokenID_) external;\n\n    function burnToken(address from_, uint256 tokenID_, uint256 amount_) external;\n\n    function invalidateToken(uint256 tokenID_) external;\n\n    function consumeAndValidateSignature(\n        TypedData.StoragePTR memory storageData,\n        bytes memory extraData,\n        bytes memory signature\n    ) external returns (address);\n\n    function owner() external view returns (address);\n\n    function getStorageName(uint8 storageType_) external view returns (string memory);\n\n    function validateStorageData(TypedData.StoragePTR memory storageData_) external view;\n\n    function recoverStorageSigner(\n        TypedData.StoragePTR memory storageData,\n        bytes memory extraData,\n        bytes memory signature\n    ) external view returns (address);\n\n    function createMetadata(\n        uint256 tokenID_,\n        uint8 tokenType_\n    ) external view returns (string memory);\n\n    function getTokenType(uint256 tokenID_) external view returns (uint8);\n\n    function tokenTypeRegistry(uint8 tokenType_) external view returns (address);\n\n    function coaNFT() external view returns (address);\n}\n"

    },

    "contracts/interfaces/ICOANFT.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\ninterface ICOANFT {\n    function mint(address to_, uint256 amount_, uint8 tokenType_) external returns (uint256);\n\n    function clone(address to_, uint256 tokenID_, uint256 amount_) external;\n\n    function burn(address from_, uint256 tokenID_, uint256 amount_) external;\n\n    function invalidate(uint256 tokenID_) external;\n\n    function getType(uint256 tokenID_) external view returns (uint8);\n}\n"

    },

    "contracts/interfaces/IIdentityV1.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\nimport \"contracts/libs/TypedData.sol\";\n\ninterface IIdentityV1 {\n    function readIdentity(uint256 tokenID_) external view returns (TypedData.IdentityV1 memory);\n}\n"

    },

    "contracts/libs/Constants.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\n// keccak256(\"CREATE_ROLE\") = '0x2ad5dda4abad2eba9015076cc6a1db468a91b444067d58b590c8c73985521a04'\nbytes32 constant CREATE_ROLE = keccak256(\"CREATE_ROLE\"); // role to create new identities\nbytes32 constant UPDATE_DELETE_ROLE = keccak256(\"UPDATE_DELETE_ROLE\"); // role to update or delete identities and certificates\n"

    },

    "contracts/libs/Errors.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\nerror SignatureNotUnique(bytes signature); // error for when the signature is not unique\nerror InvalidIdentityOwner(address owner); // error for when the identity owner is invalid\nerror InvalidIdentityAuthority(address owner); // error for when the identity authority is invalid\nerror IdentityDoesNotExist(uint256 tokenID); // error for when the identity does not exist\nerror CertificateDoesNotExistOrInvalid(uint256 tokenID); // error for when the certificate does not exist\nerror InvalidSignature(uint256 nonce, bytes signature); // error for when the signature is invalid\nerror CannotTransferIdentity(uint256 tokenID); // error for when the identity cannot be transferred\nerror NotAuthorized(address sender, uint256 tokenID); // error for when the sender is not authorized\nerror TokenDoesNotExist(uint256 tokenID); // error for when the token does not exist\nerror MismatchedInputLengths(); // error for when the input lengths do not match\nerror NotOwnerOrCOA(); // error for when the sender is not the owner or COA\nerror TokenTypeDoesNotExist(uint8 tokenType); // error for when the token type does not exist\nerror NotEnoughBalance(address owner, uint256 tokenID); // error for when the owner does not have enough balance\nerror NotOwner(); // error for when the sender is not the owner\nerror StorageTypeDoesNotExist(uint8 storageType_);\nerror NotUpdateDeleteRole(address sender);\nerror InvalidAuthorityID(); // error for when the authority id is invalid\nerror TokenIsNotInvalid();\nerror TokenNotCreatedYet(uint256 tokenID_);\nerror InvalidStorageType();\nerror InvalidTokenTypeOrAddress();\nerror NotAllowedToPerformOperationsWithThisType(uint256 tokenType_);\nerror InvalidTokenType();\nerror ZeroAddress();\n"

    },

    "contracts/libs/TypedData.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\n\nlibrary TypedData {\n    enum DataType {\n        INVALID,\n        IDENTITY_V1,\n        CERTIFICATE_V1\n    }\n\n    enum StorageType {\n        INVALID,\n        URI,\n        IPFS,\n        ARWEAVE,\n        FILE_COIN\n    }\n\n    struct TokenData {\n        uint8 dataType;\n        bytes data;\n    }\n\n    struct IdentityV1 {\n        address owner;\n        StoragePTR storagePTR;\n        address authority;\n    }\n\n    struct CertificateV1 {\n        StoragePTR storagePTR;\n        uint256 authorityIdentityId;\n    }\n\n    struct Signature {\n        uint256 nonce;\n        bytes signatureRaw;\n    }\n\n    struct StoragePTR {\n        uint8 storageType;\n        string identifier;\n    }\n\n    function isValid(StoragePTR memory strg) internal pure returns (bool) {\n        return\n            strg.storageType != 0 &&\n            bytes(strg.identifier).length != 0 &&\n            keccak256(abi.encodePacked(strg.identifier)) != keccak256(abi.encodePacked(\"\"));\n    }\n}\n"

    },

    "contracts/types/CertificateV1.sol": {

      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.19;\nimport \"contracts/libs/TypedData.sol\";\nimport \"contracts/interfaces/ICOA.sol\";\nimport \"contracts/libs/Errors.sol\";\nimport \"contracts/interfaces/ICOA.sol\";\nimport \"contracts/interfaces/IIdentityV1.sol\";\nimport \"@openzeppelin/contracts/utils/Strings.sol\";\nimport \"contracts/libs/Constants.sol\";\nimport \"contracts/libs/TypedData.sol\";\n\ncontract CertificateV1 {\n    using Strings for uint256;\n    using Strings for address;\n    /// address of the COA contract\n    ICOA public immutable coa;\n    /// address of the Identity contract\n    IIdentityV1 public immutable identity;\n    /// mapping of tokenID to certificate\n    mapping(uint256 => TypedData.CertificateV1) public certificates;\n\n    event CertificateCreated(\n        uint256 indexed tokenID,\n        uint256 indexed authorityIdentityId,\n        TypedData.CertificateV1 certificate\n    );\n\n    event CertificateUpdated(\n        uint256 indexed tokenID,\n        uint256 indexed authorityIdentityId,\n        TypedData.CertificateV1 certificate\n    );\n\n    event CertificateDeleted(uint256 indexed tokenID, uint256 indexed authorityIdentityId);\n\n    /**\n     * @dev Modifier to check if the certificate exists.\n     */\n    modifier certificateExists(uint256 tokenID_) {\n        if (!_isCertificateValid(tokenID_)) {\n            revert CertificateDoesNotExistOrInvalid(tokenID_);\n        }\n        _;\n    }\n\n    /**\n     * @dev Modifier to check if the caller has the right to create certificates directly.\n     */\n    modifier onlyCreatorRole() {\n        if (!coa.hasRole(CREATE_ROLE, msg.sender)) {\n            revert NotOwner();\n        }\n        _;\n    }\n\n    /**\n     * @dev Modifier to check if an address has the right to update or delete a certificate.\n     */\n    modifier onlyUpdateDeleteRole() {\n        if (!coa.hasRole(UPDATE_DELETE_ROLE, msg.sender)) {\n            revert NotUpdateDeleteRole(msg.sender);\n        }\n        _;\n    }\n\n    /**\n     * @dev Modifier to check if an address has the right to create a certificate.\n     */\n    modifier isAuthorizedForCloning(uint256 tokenID_) {\n        TypedData.CertificateV1 memory cert = certificates[tokenID_];\n        TypedData.IdentityV1 memory certIdentity = identity.readIdentity(cert.authorityIdentityId);\n        if (certIdentity.owner != msg.sender && !coa.hasRole(CREATE_ROLE, msg.sender)) {\n            revert NotAuthorized(msg.sender, tokenID_);\n        }\n        _;\n    }\n\n    /**\n     * @dev Modifier to check if an address has the right to create a certificate.\n     */\n    modifier isAuthorizedForCreation(uint256 identityID_) {\n        TypedData.IdentityV1 memory certIdentity = identity.readIdentity(identityID_);\n        if (certIdentity.owner != msg.sender && !coa.hasRole(CREATE_ROLE, msg.sender)) {\n            revert NotAuthorized(msg.sender, identityID_);\n        }\n        _;\n    }\n\n    /**\n     * @dev Modifier to check if an address has the right to create a certificate.\n     */\n    modifier isAuthorizedForModification(uint256 tokenID_) {\n        TypedData.CertificateV1 memory cert = certificates[tokenID_];\n        TypedData.IdentityV1 memory certIdentity = identity.readIdentity(cert.authorityIdentityId);\n        if (certIdentity.owner != msg.sender && !coa.hasRole(UPDATE_DELETE_ROLE, msg.sender)) {\n            revert NotAuthorized(msg.sender, tokenID_);\n        }\n        _;\n    }\n\n    constructor(address coa_, address identity_) {\n        coa = ICOA(coa_);\n        identity = IIdentityV1(identity_);\n    }\n\n    /**\n     * @notice Creates multiple new certificates using the data signed by the CREATE_ROLE.\n     * @param to_ The owner of the certificate to mint.\n     * @param certificate_ The certificates to mint.\n     * @param signature_ The signatures of the certificates from signer with CREATE_ROLE.\n     */\n    function createCertificateWithSignature(\n        TypedData.CertificateV1[] calldata certificate_,\n        address[] calldata to_,\n        TypedData.Signature[] calldata signature_\n    ) public {\n        if (\n            to_.length != certificate_.length ||\n            certificate_.length != signature_.length ||\n            to_.length == 0\n        ) {\n            revert MismatchedInputLengths();\n        }\n        // create each certificate\n        for (uint256 i = 0; i < certificate_.length; i++) {\n            _createCertificateWithSignature(certificate_[i], to_[i], signature_[i]);\n        }\n    }\n\n    /**\n     * @notice Creates multiple new certificates. The caller must have the CREATE_ROLE or be the owner of the identity.\n     * @param to_ The owner of the certificate to mint.\n     * @param certificate_ The certificates to mint.\n     */\n    function createCertificate(\n        TypedData.CertificateV1[] calldata certificate_,\n        address[] calldata to_\n    ) public {\n        if (to_.length != certificate_.length || to_.length == 0) {\n            revert MismatchedInputLengths();\n        }\n        // create each certificate\n        for (uint256 i = 0; i < certificate_.length; i++) {\n            _createCertificate(certificate_[i], to_[i]);\n        }\n    }\n\n    /**\n     * @notice Clones an existing certificate.\n     * @param to_  The address in which the certificate will be minted/cloned.\n     * @param tokenID_ The tokenID of the certificate to be cloned\n     * @param amount_  The amount of the certificate to mint/clone.\n     */\n    function cloneCertificate(\n        address to_,\n        uint256 tokenID_,\n        uint256 amount_\n    ) public certificateExists(tokenID_) onlyCreatorRole {\n        coa.cloneToken(to_, tokenID_, amount_);\n    }\n\n    /**\n     * @notice Updates the storagePTR of a certificate.\n     * @param tokenID_ The tokenID of the certificate to be updated\n     * @param storagePTR_ the new storagePTR of the certificate\n     */\n    function updateCertificate(\n        uint256 tokenID_,\n        TypedData.StoragePTR memory storagePTR_\n    ) public certificateExists(tokenID_) onlyUpdateDeleteRole {\n        // check that the certificate has a valid storage\n        coa.validateStorageData(storagePTR_);\n        // update the certificate\n        certificates[tokenID_].storagePTR = storagePTR_;\n        emit CertificateUpdated(\n            tokenID_,\n            certificates[tokenID_].authorityIdentityId,\n            certificates[tokenID_]\n        );\n    }\n\n    /**\n     * @notice Invalidates a certificate. The certificate will return a deleted asset metadata but\n     * the ERC1155 tokens will still exist. The users of the invalidated certificate will be able to\n     * burn their tokens using the burnCertificate function.\n     * @param tokenID_ the tokenID of the certificate\n     */\n    function invalidateCertificate(\n        uint256 tokenID_\n    ) public certificateExists(tokenID_) isAuthorizedForModification(tokenID_) {\n        // invalidate the token\n        coa.invalidateToken(tokenID_);\n        emit CertificateDeleted(tokenID_, certificates[tokenID_].authorityIdentityId);\n    }\n\n    /**\n     * @notice Burn the ERC1155 tokens of a certificate. Even if all tokens are burned, the\n     * certificate may still be valid as long its not invalidated.\n     * @param from_ the address of the owner of the tokens\n     * @param tokenID_ the tokenID of the certificate\n     * @param amount_ the amount of tokens to burn\n     */\n    function burnCertificate(\n        address from_,\n        uint256 tokenID_,\n        uint256 amount_\n    ) public certificateExists(tokenID_) onlyUpdateDeleteRole {\n        coa.burnToken(from_, tokenID_, amount_);\n    }\n\n    /**\n     * @notice returns the certificate of a tokenID\n     * @param tokenID_ the tokenID of the certificate\n     */\n    function readCertificate(\n        uint256 tokenID_\n    ) public view certificateExists(tokenID_) returns (TypedData.CertificateV1 memory) {\n        return certificates[tokenID_];\n    }\n\n    /**\n     * @notice returns the certificates of an array of tokenIDs\n     * @param tokenIDs_ the tokenIDs of the certificates\n     */\n    function readCertificatesBatch(\n        uint256[] calldata tokenIDs_\n    ) public view returns (TypedData.CertificateV1[] memory) {\n        TypedData.CertificateV1[] memory certs = new TypedData.CertificateV1[](tokenIDs_.length);\n        for (uint256 i = 0; i < tokenIDs_.length; i++) {\n            certs[i] = readCertificate(tokenIDs_[i]);\n        }\n        return certs;\n    }\n\n    /**\n     * @notice returns the metadata of a certificate\n     * @param tokenID_ the tokenID of the certificate\n     */\n    function createMetadata(uint256 tokenID_) public view returns (string memory) {\n        TypedData.CertificateV1 memory cert = certificates[tokenID_];\n        TypedData.IdentityV1 memory certIdentity = identity.readIdentity(cert.authorityIdentityId);\n        TypedData.StoragePTR memory storagePTR = cert.storagePTR;\n        string memory storageType = coa.getStorageName(storagePTR.storageType);\n        return\n            string(\n                abi.encodePacked(\n                    '{\"name\":\"Valence Certificate\",\"description\":\"These are certificates made by a registered identity\",\"attributes\":[{\"trait_type\":\"Creator\",\"value\":\"',\n                    certIdentity.owner.toHexString(),\n                    '\"},{\"trait_type\":\"CreatorIdentityID\",\"value\":\"',\n                    cert.authorityIdentityId.toHexString(),\n                    '\"},{\"trait_type\":\"Storage Type\",\"value\":\"',\n                    storageType,\n                    '\"},{\"trait_type\":\"Storage Identifier\",\"value\":\"',\n                    storagePTR.identifier,\n                    '\"}]}'\n                )\n            );\n    }\n\n    /**\n     * @notice checks if a certificate is valid\n     * @param tokenID_ the