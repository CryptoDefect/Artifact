    mapping(address => bool) public erc721s;
    address public signer;
    uint public nextDepositId = 0;
    mapping(uint => DepositInfo) public deposits;

    // event
    event ERC721(address erc721, bool status);
    event Deposit(address depositor, uint depositId, address erc721, uint tokenId, string extData);
    event Withdraw(uint depositId, address erc721, uint tokenId);

    //
    constructor()
        EIP712("LuckyNFTPool", "1")
    {
    }

    function setOpen(
        bool open_
    )
    external
    onlyOwner
    {
        open = open_;
    }

    function setSigner(
        address signer_
    )
    external
    onlyOwner
    {
        signer = signer_;
    }

    function setERC721(
        address erc721,
        bool status
    )
    external
    onlyOwner
    {
        erc721s[erc721] = status;
        emit ERC721(erc721, status);
    }

    function deposit(
        address erc721,
        uint tokenId,
        string memory extData
    )
    external
    nonReentrant
    {
        require(erc721s[erc721], "invalid ERC721 status");
        _deposit(erc721, tokenId, extData);
    }

