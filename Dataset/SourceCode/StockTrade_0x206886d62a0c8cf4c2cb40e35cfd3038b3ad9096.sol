    "contracts/swisscheese/trade/StockToken.sol": {

      "content": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.13;\n\nimport {ERC20} from \"@openzeppelin/contracts/token/ERC20/ERC20.sol\";\nimport {IERC20} from \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport {Ownable} from \"@openzeppelin/contracts/access/Ownable.sol\";\nimport {ReentrancyGuard} from \"@openzeppelin/contracts/security/ReentrancyGuard.sol\";\nimport {IStockToken} from \"./interfaces/IStockToken.sol\";\nimport {Permissioned} from \"../common/Permissioned.sol\";\nimport {IPermissionManager} from \"../common/interfaces/IPermissionManager.sol\";\nimport {Initializable} from \"../common/Initializable.sol\";\n\n/// @title StockToken\n/// @author DeepchainLabs\n/// @notice This contract is the ERC20 token for the stock\ncontract StockToken is\n    ERC20,\n    Permissioned,\n    ReentrancyGuard,\n    IStockToken,\n    Initializable\n{\n    event StockTradeAddressUpdated(address indexed _address);\n\n    address private stockTradeAddress;\n\n    string private __name;\n    string private __symbol;\n\n    constructor() ERC20(\"\", \"\") {}\n\n    /// initialize function to initialize the contract\n    /// @param tokenName name of the token\n    /// @param tokenSymbol symbol of the token\n    /// @param initialStockTradeAddress stockTrade contract address\n    /// @param initialPermissionManagerAddress permission manager address\n    function initialize(\n        string memory tokenName,\n        string memory tokenSymbol,\n        address initialStockTradeAddress,\n        address initialPermissionManagerAddress\n    ) external override initializer {\n        require(\n            initialStockTradeAddress != address(0),\n            \"Invalid stock trade address\"\n        );\n        require(\n            initialStockTradeAddress != address(0),\n            \"Invalid stock trade address\"\n        );\n        stockTradeAddress = initialStockTradeAddress;\n        __name = tokenName;\n        __symbol = tokenSymbol;\n        initializePermissioned(initialPermissionManagerAddress);\n        emit StockTradeAddressUpdated(initialStockTradeAddress);\n    }\n\n    /// Returns the stock token name\n    /// @return (string) token name\n    function name() public view override returns (string memory) {\n        return __name;\n    }\n\n    /// Returns the stock token symbol\n    /// @return (string) token symbol\n    function symbol() public view override returns (string memory) {\n        return __symbol;\n    }\n\n    modifier onlyPermissionedOrTradeManager() {\n        require(\n            IPermissionManager(getPermissionManagerAddress()).isManagerOrAdmin(\n                msg.sender\n            ) || msg.sender == stockTradeAddress,\n            \"StockToken: caller is not the owner or trade manager\"\n        );\n        _;\n    }\n\n    /// update the address of stock trade manager\n    /// @param newStockTradeAddress (address) new stock trade address\n    function updateStockTradeAddress(\n        address newStockTradeAddress\n    ) external onlyAdminOrManager {\n        require(newStockTradeAddress != address(0), \"StockToken: zero address\");\n        stockTradeAddress = newStockTradeAddress;\n        emit StockTradeAddressUpdated(newStockTradeAddress);\n    }\n\n    /// mint new stock token, only trade manager/admin/permissioned manager can call this function\n    /// @param to mint target\n    /// @param amount mint amount\n    function mint(\n        address to,\n        uint256 amount\n    ) external override onlyPermissionedOrTradeManager nonReentrant {\n        _mint(to, amount);\n    }\n\n    /// burn stock token, only trade manager/admin/permissioned manager can call this function\n    /// @param from the address to burn the token from\n    /// @param amount the amount to burn\n    function burn(\n        address from,\n        uint256 amount\n    ) external override onlyPermissionedOrTradeManager nonReentrant {\n        _burn(from, amount);\n    }\n\n    /// User can burn their own token if they choose to\n    /// @param amount the amount to burn\n    function burnOwn(uint256 amount) external nonReentrant {\n        _burn(msg.sender, amount);\n    }\n\n    /// @dev    fallback function to revert any send call\n    receive() external payable {\n        revert();\n    }\n}\n"

    },

    "contracts/swisscheese/trade/StockTrade.sol": {

      "content": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.13;\n\nimport {IERC20} from \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport {ReentrancyGuard} from \"@openzeppelin/contracts/security/ReentrancyGuard.sol\";\nimport {StockToken} from \"./StockToken.sol\";\nimport {IStockToken} from \"./interfaces/IStockToken.sol\";\nimport {ECDSA} from \"@openzeppelin/contracts/utils/cryptography/ECDSA.sol\";\nimport {Permissioned} from \"../common/Permissioned.sol\";\n\n/// @title StockTrade\n/// @author DeepchainLabs\n/// @notice This contract manages all the trading on Swisschese Trading\ncontract StockTrade is ReentrancyGuard, Permissioned {\n    using ECDSA for bytes32;\n\n    event StockTokenCreated(\n        address indexed tokenAddress,\n        string name,\n        string symbol\n    );\n    event StockTokenListModified(address indexed tokenAddress, bool isEnabled);\n    event StockTraded(\n        address indexed stockTokenAddress,\n        address indexed currencyAddress,\n        address indexed userAddress,\n        uint256 stockTokenAmount,\n        uint256 currencyAmount,\n        string buyOrSell\n    );\n    event FundsWithdrawn(\n        address indexed toAddress,\n        address indexed currencyAddress,\n        uint256 amount\n    );\n\n    event NativeTokenReceived(address indexed fromAddress, uint256 amount);\n\n    address public signerWalletAddress;\n    mapping(address => bool) public isStockToken;\n    mapping(bytes => bool) public isSignatureUsed;\n    uint256 public signatureValidityDurationInSeconds = 90;\n\n    constructor(\n        address initialSignerWalletAddress,\n        address permissiontManagerAddress\n    ) {\n        require(\n            initialSignerWalletAddress != address(0),\n            \"StockTrade: zero address for signer wallet address\"\n        );\n        initializePermissioned(permissiontManagerAddress);\n        signerWalletAddress = initialSignerWalletAddress;\n    }\n\n    /// Create a new stock token and add it to the list of valid stock tokens\n    /// @param tokenName (string) name of the new stock token\n    /// @param tokenSymbol (string) symbol of the new stock token\n    function createStockToken(\n        string memory tokenName,\n        string memory tokenSymbol\n    ) external onlyAdminOrManager nonReentrant returns (address addr) {\n        bytes memory bytecode = type(StockToken).creationCode;\n        bytes32 salt = keccak256(\n            abi.encodePacked(\n                tokenName,\n                block.number,\n                tokenSymbol,\n                block.timestamp\n            )\n        );\n        assembly {\n            addr := create2(0, add(bytecode, 32), mload(bytecode), salt)\n            if iszero(extcodesize(addr)) {\n                revert(0, 0)\n            }\n        }\n        IStockToken(addr).initialize(\n            tokenName,\n            tokenSymbol,\n            address(this),\n            getPermissionManagerAddress()\n        );\n        isStockToken[addr] = true;\n        emit StockTokenCreated(addr, tokenName, tokenSymbol);\n        emit StockTokenListModified(addr, true);\n    }\n\n    /// Update the signature validity duration in seconds\n    /// @param newSignatureValidityDurationInSeconds (uint256) new signature validity duration in seconds\n    function setSignatureValidityDurationInSeconds(\n        uint256 newSignatureValidityDurationInSeconds\n    ) external onlyAdminOrManager {\n        signatureValidityDurationInSeconds = newSignatureValidityDurationInSeconds;\n    }\n\n    /// Update stock token to be enabled or disabled\n    /// @param stockTokenAddress (address) address of the stock token\n    /// @param isEnabled (bool) whether the stock token is enabled or not\n    function updateStockTokenConfig(\n        address stockTokenAddress,\n        bool isEnabled\n    ) external onlyAdminOrManager {\n        require(\n            isStockToken[stockTokenAddress] != isEnabled,\n            \"StockTrade: already desired config set\"\n        );\n        isStockToken[stockTokenAddress] = isEnabled;\n        emit StockTokenListModified(stockTokenAddress, isEnabled);\n    }\n\n    function validateERC20Balance(\n        address tokenAddress,\n        address ownerAddress,\n        uint256 leastAmount,\n        string memory errorMessage\n    ) private view {\n        require(\n            IERC20(tokenAddress).balanceOf(ownerAddress) >= leastAmount,\n            errorMessage\n        );\n    }\n\n    function validateERC20Allowance(\n        address tokenAddress,\n        address ownerAddress,\n        address spenderAddress,\n        uint256 leastAmount,\n        string memory errorMessage\n    ) private view {\n        require(\n            IERC20(tokenAddress).allowance(ownerAddress, spenderAddress) >=\n                leastAmount,\n            errorMessage\n        );\n    }\n\n    function transferERC20From(\n        address tokenAddress,\n        address fromAddress,\n        address toAddress,\n        uint256 amount,\n        string memory insufficientBalanceErrorMessage,\n        string memory insufficientAllowanceErrorMessage\n    ) private {\n        validateERC20Balance(\n            tokenAddress,\n            fromAddress,\n            amount,\n            insufficientBalanceErrorMessage\n        );\n        validateERC20Allowance(\n            tokenAddress,\n            fromAddress,\n            toAddress,\n            amount,\n            insufficientAllowanceErrorMessage\n        );\n        require(\n            IERC20(tokenAddress).transferFrom(fromAddress, toAddress, amount),\n            \"StockTrade: transfer failed\"\n        );\n    }\n\n    function transferERC20(\n        address tokenAddress,\n        address toAddress,\n        uint256 amount\n    ) private {\n        require(\n            IERC20(tokenAddress).transfer(toAddress, amount),\n            \"StockTrade: transfer failed\"\n        );\n    }\n\n    modifier validateSignature(\n        bytes memory signature,\n        address stocktokenAddress,\n        uint256 stockTokenAmount,\n        address currencyAddress,\n        uint256 currencyAmount,\n        string memory buyOrSell,\n        uint256 timestamp\n    ) {\n        require(!isSignatureUsed[signature], \"Signature already redeemed\");\n        require(\n            block.timestamp <= timestamp + signatureValidityDurationInSeconds,\n            \"Signature expired, please try again\"\n        );\n        require(\n            keccak256(\n                abi.encodePacked(\n                    msg.sender,\n                    stocktokenAddress,\n                    stockTokenAmount,\n                    currencyAddress,\n                    currencyAmount,\n                    buyOrSell,\n                    timestamp\n                )\n            ).toEthSignedMessageHash().recover(signature) ==\n                signerWalletAddress,\n            \"Invalid Signature\"\n        );\n        isSignatureUsed[signature] = true;\n        _;\n    }\n\n    /// Buy stock token\n    /// @param stocktokenAddress (address) address of the stock token address\n    /// @param stockTokenAmount (uint256) amount of stock token to buy\n    /// @param currencyAddress (address) address of the currency to pay with\n    /// @param currencyAmount (uint256) amount of currency to pay with\n    /// @param signature (bytes) a system generated signature to validate the pricing\n    /// @param timestamp (uint256) timestamp of generating the signature\n    function buyStock(\n        address stocktokenAddress,\n        uint256 stockTokenAmount,\n        address currencyAddress,\n        uint256 currencyAmount,\n        bytes memory signature,\n        uint256 timestamp\n    )\n        external\n        payable\n        nonReentrant\n        validateSignature(\n            signature,\n            stocktokenAddress,\n            stockTokenAmount,\n            currencyAddress,\n            currencyAmount,\n            \"buy\",\n            timestamp\n        )\n    {\n        require(isStockToken[stocktokenAddress], \"Invalid Stock Token\");\n        if (currencyAddress == address(0)) {\n            // Native Token\n            require(\n                msg.value >= currencyAmount,\n                \"Not enough native token provided to complete the purchase\"\n            );\n        } else {\n            // ERC20 Token\n            transferERC20From(\n                currencyAddress,\n                msg.sender,\n                address(this),\n                currencyAmount,\n                \"You do not have enough balance to complete the purchase\",\n                \"You have not approved the contract to spend the token\"\n            );\n        }\n        IStockToken(stocktokenAddress).mint(msg.sender, stockTokenAmount);\n        emit StockTraded(\n            stocktokenAddress,\n            currencyAddress,\n            msg.sender,\n            stockTokenAmount,\n            currencyAmount,\n            \"buy\"\n        );\n    }\n\n    /// Sell a stock\n    /// @param stocktokenAddress (address) address of the stock token to sell\n    /// @param stockTokenAmount (uint256) amount of stock token to sell\n    /// @param currencyAddress (address) address of the currency to receive\n    /// @param currencyAmount (uin256) amount of currency to receive\n    /// @param signature (bytes) system generated signature to validate pricing\n    /// @param timestamp (uint256) timestamp of generating the signature\n    function sellStock(\n        address stocktokenAddress,\n        uint256 stockTokenAmount,\n        address currencyAddress,\n        uint256 currencyAmount,\n        bytes memory signature,\n        uint256 timestamp\n    )\n        external\n        nonReentrant\n        validateSignature(\n            signature,\n            stocktokenAddress,\n            stockTokenAmount,\n            currencyAddress,\n            currencyAmount,\n            \"sell\",\n            timestamp\n        )\n    {\n        require(isStockToken[stocktokenAddress], \"Invalid Stock Token\");\n        IStockToken(stocktokenAddress).burn(msg.sender, stockTokenAmount);\n        if (currencyAddress == address(0)) {\n            // Native Token\n            require(\n                currencyAmount <= address(this).balance,\n                \"Not enough supply of Native Token\"\n            );\n            payable(msg.sender).transfer(currencyAmount);\n        } else {\n            transferERC20(currencyAddress, msg.sender, currencyAmount);\n        }\n        emit StockTraded(\n            stocktokenAddress,\n            currencyAddress,\n            msg.sender,\n            stockTokenAmount,\n            currencyAmount,\n            \"sell\"\n        );\n    }\n\n    /// Withdraw funds from the contract, only for admins\n    /// @param currencyAddress (address) currency address to withdraw, zero address for native token\n    /// @param amount (uint256) aount of token to withdraw\n    /// @param toAddress (address) reveiver address of the withdrawn tokens\n    function withdrawFunds(\n        address currencyAddress,\n        uint256 amount,\n        address payable toAddress\n    ) external nonReentrant onlyAdmin {\n        require(toAddress != address(0), \"Invalid to Address\");\n        if (currencyAddress == address(0)) {\n            // Native Token\n            require(\n                amount <= address(this).balance,\n                \"Not enough supply of Native Token\"\n            );\n            payable(toAddress).transfer(amount);\n        } else {\n            // ERC20 Token\n            transferERC20(currencyAddress, toAddress, amount);\n        }\n        emit FundsWithdrawn(toAddress, currencyAddress, amount);\n    }\n\n    receive() external payable {\n        emit NativeTokenReceived(msg.sender, msg.value);\n    }\n}\n"

    }

