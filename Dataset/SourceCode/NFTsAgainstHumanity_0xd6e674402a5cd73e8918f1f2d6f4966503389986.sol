    struct Card { 
      string name;
      string description;
      string black;
      string white;
   }
  
    mapping(address => uint256) private _mintedFreeAmount;
    mapping (uint256 => Card) public cards;

    constructor() ERC721A("NFTs Against Humanity", "NAH") {}

    function mint(uint256 count) external payable {
        uint256 supply = totalSupply();
    
        for (uint256 i = 0; i < count; i++) {
            cards[supply + i] = Card(
                string(abi.encodePacked('NFTs Against Humanity #', uint256(supply + i + 1).toString())),
                "Are you ready to play good old fun fill-in-the-blank card game fully on-chain? NFTs Against Humanity is a simple yet the most inappropriate block-chain game ever made. And it all miraculously happens fully on-chain on the wonderful Ethereum blockchain.",
                blackCards[randomNum(blackCards.length, block.timestamp, supply + i)],
                whiteCards[randomNum2(whiteCards.length, block.difficulty, supply + i)]
            );
        }
        
        bool isFree = ((totalFreeMinted + count < totalFree + 1) &&
            (_mintedFreeAmount[msg.sender] < maxFreePerWallet));
        
        require(mintEnabled, "Mint is not live yet");
        require(count <= maxPerTx, "Max per TX reached.");
        require(totalSupply() + count <= maxSupply, "Sold out");

        if (isFree) { 
            uint256 freeLeft = (maxFreePerWallet - _mintedFreeAmount[msg.sender]);
            uint256 freeAmount = (count >= freeLeft ? freeLeft : count);

            require(msg.value >= (count - freeAmount) * price, "Please send the exact ETH amount");
            
            _mintedFreeAmount[msg.sender] += freeAmount;
            totalFreeMinted += freeAmount;
        } else {
            require(msg.value >= count * price, "Please send the exact ETH amount");
        }

        _safeMint(msg.sender, count);
    }

    function randomNum(uint256 _mod, uint256 _seed, uint _salt) public view returns(uint256) {
        return uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, _seed, _salt))) % _mod;
    }

    function randomNum2(uint256 _mod, uint256 _seed, uint _salt) public view returns(uint256) {
        return uint(keccak256(abi.encodePacked(block.difficulty, msg.sender, _seed, _salt))) % _mod;
    }

  
    function buildImage(Card memory currentCard) public pure returns(string memory) {
        return Base64.encode(bytes(
            abi.encodePacked(
              '<svg width="500" height="500" xmlns="http://www.w3.org/2000/svg">',
              '<style xmlns="http://www.w3.org/2000/svg">.white { fill: white; font-family: sans-serif; font-weight: bold; }</style>'
              '<rect height="500" width="500" fill="#ffffff"/>',
              '<defs><filter x="0" y="0" width="1" height="1" id="solid"><feFlood flood-color="black" result="bg" /><feMerge><feMergeNode in="bg"/><feMergeNode in="SourceGraphic"/></feMerge></filter></defs>',
              '<foreignObject x="5%" y="30" width="450" height="200" class="white" style="font-size:28px"><span xmlns="http://www.w3.org/1999/xhtml">',currentCard.black,'</span></foreignObject>',
              '<text x="5%" y="450" class="white" style="font-size:20px" filter="url(#solid)">',currentCard.white,'</text>',
              '</svg>'
          )
      ));
    }
  
    function buildMetadata(Card memory currentCard) public pure returns(string memory) {
        return string(abi.encodePacked(
                'data:application/json;base64,', Base64.encode(bytes(abi.encodePacked(
                            '{"name":"', 
                            currentCard.name,
                            '", "description":"', 
                            'Are you ready to play good old fun fill-in-the-blank card game fully on-chain? NFTs Against Humanity is a simple yet the most inappropriate block-chain game ever made. And it all miraculously happens fully on-chain on the wonderful Ethereum blockchain.',
                            '", "image": "', 
                            'data:image/svg+xml;base64,', 
                            buildImage(currentCard),
                            '"}')))));
    }

    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
        require(_exists(_tokenId),"ERC721Metadata: URI query for nonexistent token");
        return buildMetadata(cards[_tokenId]);
    }

    function setFreeAmount(uint256 amount) external onlyOwner {
        totalFree = amount;
    }

    function setPrice(uint256 _newPrice) external onlyOwner {
        price = _newPrice;
    }

    function toggleMint() external onlyOwner {
        mintEnabled = !mintEnabled;
    }

    function withdraw() external onlyOwner {
        require(payable(msg.sender).send(address(this).balance));
    }
