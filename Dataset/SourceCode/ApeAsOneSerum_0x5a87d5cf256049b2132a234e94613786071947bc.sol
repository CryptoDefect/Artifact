        NFTCollectionWhitelistReserved._mintAmount(_amount);
    }

    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override(ERC721A, NFTCollectionRoyalties)
        returns (bool)
    {
        return super.supportsInterface(_interfaceId);
    }
}

