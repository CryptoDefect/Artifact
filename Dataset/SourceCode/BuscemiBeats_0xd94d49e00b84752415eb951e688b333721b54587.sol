// Smart Contract by EverythingWeb3 Limited trading as Web3Hippo (web3hippo.io) in collaboration with OneGrid
// Main NFT Contract
contract BuscemiBeats is ERC721, AccessControl {
  using Strings for uint256;
  using Counters for Counters.Counter;

  // Setting up Roles for Access Control
  bytes32 public constant META49_ROLE = keccak256("49META_ROLE");

  Counters.Counter private supply;

  mapping(address => bool) public whitelist;

  string private uriPrefix = "";
  string public hiddenUriPrefix = "ipfs://QmWYemigzSZ2rdAuA6AJET8dHMEdPQuAdCuK4P4uAsReff/";
  string public uriSuffix = ".json";
  
  uint256 public cost = 0.075 ether;
  uint256 public constant maxSupply =  3000;
  uint256 public maxPremintSupply = 2000;
  uint256 public maxMintAmountPerTx = 10;

  bool public paused = true;
  bool public premint = true;
  bool public allowListOnly = true;
  bool public revealed = false;

  address public treasuryWallet;

  constructor(address _functionsWallet, address _treasuryWallet) ERC721("Buscemi Beats", "BUSCEMI BEATS") {
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(META49_ROLE, _functionsWallet);
    treasuryWallet = _treasuryWallet;
  }
  
  // Checks to be carried out before minting tokens
  modifier mintCompliance(uint256 _mintPackages) {
    if (premint) {
        require(_mintPackages > 0 && (_mintPackages * 5) <= maxMintAmountPerTx, "Invalid mint amount!");
        require(supply.current() + (_mintPackages * 5) <= maxPremintSupply, "Max premint supply exceeded!");
    } else {
        require(_mintPackages > 0, "Invalid mint amount!");
        require(supply.current() + (_mintPackages * 5) <= maxSupply, "Max supply exceeded!");
    }
    _;
  }

  // main minting function
  function mint(uint256 _mintPackages) public payable mintCompliance(_mintPackages) {
    require(!paused, "The contract is paused!");
    require(msg.value >= cost, "Insufficient funds supplied!");

    if (premint && allowListOnly) {
      require(whitelist[msg.sender], "Address not on the whitelist.");
      require(walletOfOwner(msg.sender).length < maxMintAmountPerTx, "Max mint amount exceeded!");
    }

    // calculate the number of sounds to mint
    uint256 _mintAmount = _mintPackages * 5;

    // loop over mint amount to mint the sound
    _mintLoop(msg.sender, _mintAmount);
  }
  
  // function used to mint directly into a wallet by the owner
  function mintForAddress(uint256 _mintPackages, address _receiver) public onlyRole(META49_ROLE) {
    uint256 _mintAmount = _mintPackages * 5;
    _mintLoop(_receiver, _mintAmount);
  }

  // function used in order to whitelist addresses to mint
  function whitelistAddresses(address[] memory addresses) public onlyRole(META49_ROLE) {
    for (uint i = 0; i < addresses.length; ++i) {
        whitelist[addresses[i]] = true;
    }
  }

  // function to remove an address from whitelist 
  function removeWhitelistAddress(address _address) public onlyRole(META49_ROLE) {
    require(_address != address(0), "Address is not valid");
    require(whitelist[_address], "Address not whitelisted");
    whitelist[_address] = false;
  }


  // function to end pre-mint phase, pause sales and update price for public sale
  function endPremint() public onlyRole(META49_ROLE) {
      premint = false;
      paused = true;
      cost = 0.095 ether;
  }

  function walletOfOwner(address _owner) public view returns (uint256[] memory) {
    uint256 ownerTokenCount = balanceOf(_owner);
    uint256[] memory ownedTokenIds = new uint256[](ownerTokenCount);
    uint256 currentTokenId = 1;
    uint256 ownedTokenIndex = 0;

    while (ownedTokenIndex < ownerTokenCount && currentTokenId <= maxSupply) {
      address currentTokenOwner = ownerOf(currentTokenId);

      if (currentTokenOwner == _owner) {
        ownedTokenIds[ownedTokenIndex] = currentTokenId;

        ownedTokenIndex++;
      }

      currentTokenId++;
    }

    return ownedTokenIds;
  }

  function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
    require(_exists(_tokenId),"URI query for nonexistent token");

    if (!revealed) {
      string memory hiddenBaseURI = _hiddenBaseURI();
      return bytes(hiddenBaseURI).length > 0
        ? string(abi.encodePacked(hiddenBaseURI, _tokenId.toString(), uriSuffix))
        : "";
    }

    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, _tokenId.toString(), uriSuffix))
        : "";
  }

  // used to call the current supply
  function totalSupply() public view returns (uint256) {
    return supply.current();
  }

  // change state of reveal
  function setRevealed(bool _state) public onlyRole(META49_ROLE) {
    revealed = _state;
  }

  // change maxPremint supply
  function setMaxPremintSupply(uint256 _amount) public onlyRole(META49_ROLE) {
    maxPremintSupply = _amount;
  }

  // function used to change the mint price
  function setCost(uint256 _cost) public onlyRole(META49_ROLE) {
    require(cost > 0, "cost can't be zero");
    cost = _cost;
  }

  function setMaxMintAmountPerTx(uint256 _maxMintAmountPerTx) public onlyRole(META49_ROLE) {
    require(_maxMintAmountPerTx > 0, "Can't set below 1");
    maxMintAmountPerTx = _maxMintAmountPerTx;
  }

  function setHiddenMetadataUri(string memory _hiddenMetadataUri) public onlyRole(META49_ROLE) {
    hiddenUriPrefix = _hiddenMetadataUri;
  }

  function setUriPrefix(string memory _uriPrefix) public onlyRole(META49_ROLE) {
    uriPrefix = _uriPrefix;
  }

  function setUriSuffix(string memory _uriSuffix) public onlyRole(META49_ROLE) {
    uriSuffix = _uriSuffix;
  }

  // function to change paused state
  function setPaused(bool _state) public onlyRole(META49_ROLE) {
    paused = _state;
  }

  // function to change premint state
  function setPremint(bool _state) public onlyRole(META49_ROLE) {
    premint = _state;
  }
  
  // function to change state of waitlist
  function updateAllowListState(bool _state) public onlyRole(META49_ROLE) {
      allowListOnly = _state;
  }

  // function used to withdraw ETH out of the contract by the owner and META49
  function withdraw() public onlyRole(META49_ROLE) {
    require(treasuryWallet != address(0), "Treasury Wallet not set");
    (bool os, ) = payable(treasuryWallet).call{value: address(this).balance}("");
    require(os, "withdraw failed");
  }

  function _mintLoop(address _receiver, uint256 _mintAmount) internal {
    for (uint256 i = 0; i < _mintAmount; ++i) {
      supply.increment();
      _safeMint(_receiver, supply.current());
    }
  }

  function _baseURI() internal view virtual override returns (string memory) {
    return uriPrefix;
  }

  function _hiddenBaseURI() internal view virtual returns (string memory) {
    return hiddenUriPrefix;
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721, AccessControl) returns (bool) {
    return super.supportsInterface(interfaceId);
  }
}

