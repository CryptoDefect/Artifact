// File contracts/NomadERC721A.sol





pragma solidity ^0.8.9;









interface NomadPassInterface {

    function balanceOf(address account, uint256 id)

    external

    view

    returns (uint256);



    function burnFromRedeemedToken(

        address account,

        uint256 id,

        uint256 amount

    ) external;

}



interface NomadRewardInterface {

    function balanceOf(address account, uint256 id)

    external

    view

    returns (uint256);



    function mint(

        address account,

        uint256 id,

        uint256 amount

    ) external;

}





contract Nomad721A is ERC721A, AccessControl, Pausable {



    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");



    NomadPassInterface private _nomadPass;

    NomadRewardInterface private _nomadReward;

    mapping(address => uint256[]) private _tokenIndexes;



    uint256 private _maxItems;

    uint256 private _maxPerTransaction;



    event TokensRedeemed(address, uint256[] ids);

    event RewardsRedeemed(address, uint256[] ids);

    event FounderBurned(address, uint256 amount);

    event CollectorBurned(address, uint256 amount);



    string private _contractURI = "";

    string private baseURI = "";



    constructor(address nomadPassAddress, uint256 maxItems, string memory name, string memory symbol, uint256 maxBatchItems) ERC721A(name, symbol, maxBatchItems) {

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        _setupRole(OPERATOR_ROLE, msg.sender);

        _nomadPass = NomadPassInterface(nomadPassAddress);

        _maxItems = maxItems;



    }





    function setContractURI(string memory newContractURI)

    external

    onlyRole(OPERATOR_ROLE)

    {

        _contractURI = newContractURI;

    }



    ///Returns the contract URI for OpenSea

    function contractURI() public view returns (string memory) {

        return _contractURI;

    }



    function exists(uint256 tokenId) public view returns (bool) {

        return super._exists(tokenId);

    }



    function setNomadPass(address nomadPassAddress)

    external

    onlyRole(OPERATOR_ROLE)

    {

        _nomadPass = NomadPassInterface(nomadPassAddress);

    }



    function setNomadReward(address nomadRewardAddress)

    external

    onlyRole(OPERATOR_ROLE)

    {

        _nomadReward = NomadRewardInterface(nomadRewardAddress);

    }



    function setMaxItems(uint256 maxItems)

    external

    onlyRole(OPERATOR_ROLE)

    {

        _maxItems = maxItems;

    }



    function setMaxPerTransaction(uint256 maxPerTransaction)

    external

    onlyRole(OPERATOR_ROLE)

    {

        _maxPerTransaction = maxPerTransaction;

    }





    function getCounter()

    external

    view

    returns (uint256)

    {

        return totalSupply();

    }



    function getTokenIndexes(address account)

    external

    view

    returns (uint256[] memory)

    {

        return _tokenIndexes[account];

    }





    function getRewardContractBalance(address account, uint256 index)

    external

    view

    returns (uint256)

    {

        require(address(_nomadReward) != address(0), "INVALID_REWARD_CONTRACT");

        return _nomadReward.balanceOf(account, index);

    }





    function _baseURI() internal view override returns (string memory) {

        return baseURI;

    }



    function setBaseURI(string memory _newBaseURI) public onlyRole(OPERATOR_ROLE) {

        baseURI = _newBaseURI;

    }





    function pause() public onlyRole(OPERATOR_ROLE) {

        _pause();

    }



    function unpause() public onlyRole(OPERATOR_ROLE) {

        _unpause();

    }



    function redeem(uint256 founderAmount, uint256 collectorAmount) external

    {

        require(!paused(), "CONTRACT_PAUSED");

        require(address(_nomadReward) != address(0), "INVALID_REWARD_CONTRACT");



        //0 is the id for Founder Pass

        require(founderAmount <= _nomadPass.balanceOf(msg.sender, 0), "INCORRECT_FOUNDER_BALANCE");



        //1 is the id for Collector Pass

        require(collectorAmount <= _nomadPass.balanceOf(msg.sender, 1), "INCORRECT_COLLECTOR_BALANCE");



        uint256 total = founderAmount + collectorAmount;

        if (_maxPerTransaction > 0) {

            require(total <= _maxPerTransaction, "INCORRECT_MAX_TRANSACTION");

        }



        require(totalSupply() + total <= _maxItems, "INCORRECT_TOTAL_BALANCE");









        //The counter for totalSupply() starts at zero

        uint256 startIndex = totalSupply();



        _safeMint(msg.sender, total, "");





        uint256[] memory ids = new uint256[](total);

        uint256 counter = 0;



        //Fill the mappings with the indexes

        for (uint256 i = startIndex; i < total + startIndex; i++) {

            _tokenIndexes[msg.sender].push(i);

            ids[counter] = i;

            counter++;

        }

        //TokensRedeemed will contain all the ids

        emit TokensRedeemed(msg.sender, ids);



        //Burn Founder passes

        if (founderAmount > 0) {

            _nomadReward.mint(msg.sender, 0, founderAmount);

            _nomadPass.burnFromRedeemedToken(msg.sender, 0, founderAmount);

            emit FounderBurned(msg.sender, founderAmount);

        }



        //Burn Collector passes

        if (collectorAmount > 0) {

            _nomadPass.burnFromRedeemedToken(msg.sender, 1, collectorAmount);

            emit CollectorBurned(msg.sender, collectorAmount);

        }



    }







    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)

    public

    view

    override(ERC721A, AccessControl)

    returns (bool)

    {

        return super.supportsInterface(interfaceId);

    }



}