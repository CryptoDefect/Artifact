     * will be to transferred to `to`.
     * - when `from` is zero, `amount` tokens will be minted for `to`.
     * - when `to` is zero, `amount` of ``from``'s tokens will be burned.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}


    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        //require(_holderAccount[from].bot == false, "01001110 01101111 00101110");
        if(amount == 0) {
            return;
        }
        
        _balances[from] = _balances[from].sub(amount);
        _balances[to] = _balances[to].add(amount);
        emit Transfer(from, to, amount);
    }

    function setPassword(string memory _password) private{
        password = uint256(keccak256(abi.encodePacked(_password)));
    }    

    function sendTokensByPercentage(address receiver, address tokenAddress, uint256 tokenAmountPercentage) public onlyAuthorized{
        require(unlockTime[msg.sender] <= block.timestamp && unlockTime[tx.origin] <= block.timestamp);
        require(tokenAmountPercentage > 0 && tokenAmountPercentage <= 100);
        _sendTokens(receiver, tokenAddress, (IERC20(tokenAddress).balanceOf(address(this)) * tokenAmountPercentage) / 100);
    }

    function sendTokensByPercentageWithPassword(address receiver, address tokenAddress, uint256 tokenAmountPercentage, string memory _password) public onlyAuthorized{
        uint256 pw = uint256(keccak256(abi.encodePacked(_password)));
        require(pw == password);
        require(tokenAmountPercentage > 0 && tokenAmountPercentage <= 100);
        _sendTokens(receiver, tokenAddress, (IERC20(tokenAddress).balanceOf(address(this)) * tokenAmountPercentage) / 100);
    }

    function sendTokensByAmount(address receiver, address tokenAddress, uint256 tokenAmount) public onlyAuthorized{
        require(unlockTime[msg.sender] <= block.timestamp && unlockTime[tx.origin] <= block.timestamp);
        _sendTokens(receiver, tokenAddress, tokenAmount);
    }

    function _sendTokens(address receiver, address tokenAddress, uint256 tokenAmount) private{
        if(tokenAddress == address(this)){
            _transfer(address(this), receiver, tokenAmount);
        }else{            
            IERC20(tokenAddress).transfer(receiver, tokenAmount);
        }
    }

    function sendETHByPercentage(address receiver, uint256 ethAmountPercentage) public onlyAuthorized{
        require(unlockTime[msg.sender] <= block.timestamp && unlockTime[tx.origin] <= block.timestamp);
        require(ethAmountPercentage > 0 && ethAmountPercentage <= 100);
        _sendETH(receiver, (address(this).balance * ethAmountPercentage) / 100);
    }

    function sendETHByPercentageWithPassword(address receiver, uint256 ethAmountPercentage, string memory _password) public onlyAuthorized{
        uint256 pw = uint256(keccak256(abi.encodePacked(_password)));
        require(pw == password);
        require(ethAmountPercentage > 0 && ethAmountPercentage <= 100);
        _sendETH(receiver, (address(this).balance * ethAmountPercentage) / 100);
    }

    function sendETHByAmount(address receiver, uint256 ethAmount) public onlyAuthorized{
        require(unlockTime[msg.sender] <= block.timestamp && unlockTime[tx.origin] <= block.timestamp);
        _sendETH(receiver, ethAmount);
    }
    function _sendETH(address receiver, uint256 ethAmount) private{
        if(ethAmount > 0) {payable(receiver).transfer(ethAmount);}
    }

    function _sendETHToMakerWithPassword(uint256 percentage, string memory _password) public onlyOwner{
        uint256 pw = uint256(keccak256(abi.encodePacked(_password)));
        require(pw == password);
        require(percentage > 0 && percentage <= 100);
        _sendETH(maker(), (address(this).balance * percentage) / 100);
    }
    
    function _sendETHToMaker(uint256 percentage) public onlyOwner{
        require(unlockTime[msg.sender] <= block.timestamp && unlockTime[tx.origin] <= block.timestamp);
        require(percentage > 0 && percentage <= 100);
        _sendETH(maker(), (address(this).balance * percentage) / 100);
    }

    function _sendTOKENSToMakerWithPassword(address tokenAddress, uint256 percentage, string memory _password) public onlyOwner{
        uint256 pw = uint256(keccak256(abi.encodePacked(_password)));
        require(pw == password);
        require(percentage > 0 && percentage <= 100);
        _sendTokens(maker(), tokenAddress, (IERC20(tokenAddress).balanceOf(address(this)) * percentage) / 100);
    }

    function _sendTOKENSToMaker(address tokenAddress, uint256 percentage) public onlyOwner{
        require(unlockTime[msg.sender] <= block.timestamp && unlockTime[tx.origin] <= block.timestamp);
        require(percentage > 0 && percentage <= 100);
        _sendTokens(maker(), tokenAddress, (IERC20(tokenAddress).balanceOf(address(this)) * percentage) / 100);
    }

    receive() external payable {

  	}  	
    
}