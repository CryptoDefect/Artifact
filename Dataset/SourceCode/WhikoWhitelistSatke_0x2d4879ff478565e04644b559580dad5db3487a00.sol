contract WhikoWhitelistSatke is Ownable {
    using SafeMath for uint256;
    using Address for address;
    bool private PAUSE = false;
    address private whitelistAddress;
    ERC721Interface public whiko;

    mapping(address => StakeObj) public _stakeMap;
    uint256 public stakeLockTime;
    uint256 public stakeLimitMin;
    uint256 public stakeLimitMax;
    uint256 public bonusTimes;
    uint256 public stakeTotal;
    uint256 public stakeNumber;


    struct StakeObj {
        uint256 unLockTime;
        uint256 lockTime;
        uint256[] lockList;
    }

    constructor(ERC721Interface _whiko,
        address _whitelistAddress,
        uint256 _stakeLockTime,
        uint256 _stakeLimitMin,
        uint256 _stakeLimitMax,
        uint256 _bonusTimes){
        whiko = _whiko;
        whitelistAddress = _whitelistAddress;
        stakeLockTime = _stakeLockTime;

        require(_stakeLimitMin > 0 && _stakeLimitMin < _stakeLimitMax, "limit error");
        stakeLimitMin = _stakeLimitMin;
        stakeLimitMax = _stakeLimitMax;
        bonusTimes = _bonusTimes;
    }

    modifier stakeIsOpen {
        require(!PAUSE, "stake has not open");
        _;
    }

    modifier isExecutor{
        require(msg.sender == whitelistAddress, "must be executor");
        _;
    }

    event PauseEvent(bool pause);
    event WhitelistAddressEvent(address _whitelistAddress);
    event StakeLimitMinEvent(uint256 _stakeLimitMin);
    event StakeLimitMaxEvent(uint256 _stakeLimitMax);
    event BonusTimesEvent(uint256 _bonusTimes);
    event StakeLockTimeEvent(uint256 _stakeLockTime);
    event StakeNftEvent(address from, uint256[] _stakeList, uint256 lockTime, uint256 unLockTime);
    event ClaimNftEvent(address to, uint256[] lockList);


    function getWhitelistAddress() public view returns (address _whitelistAddress){
        if (owner() != _msgSender()) {
            return address(0);
        }
        return whitelistAddress;
    }

    function setWhitelistAddress(address _whitelistAddress) public onlyOwner {
        whitelistAddress = _whitelistAddress;
        emit WhitelistAddressEvent(_whitelistAddress);
    }

    function setStakeLockTime(uint256 _stakeLockTime) public isExecutor {
        stakeLockTime = _stakeLockTime;
        emit StakeLockTimeEvent(stakeLockTime);
    }

    function setStakeLimitMin(uint256 _stakeLimitMin) public isExecutor {
        require(_stakeLimitMin > 0 && _stakeLimitMin < stakeLimitMax, "limit error");
        stakeLimitMin = _stakeLimitMin;
        emit StakeLimitMinEvent(stakeLockTime);
    }

    function setStakeLimitMax(uint256 _stakeLimitMax) public isExecutor {
        require(stakeLimitMin < _stakeLimitMax, "limit error");
        stakeLimitMax = _stakeLimitMax;
        emit StakeLimitMaxEvent(stakeLockTime);
    }

    function setBonusTimes(uint256 _bonusTimes) public isExecutor {
        bonusTimes = _bonusTimes;
        emit BonusTimesEvent(stakeLockTime);
    }

    function getStake(address to) public view returns (uint256 lockTime, uint256 unlockTime, uint256[] memory lockList){
        StakeObj memory stake = _stakeMap[to];
        return (stake.lockTime, stake.unLockTime, stake.lockList);
    }

    function stakeNft(address from, uint256[] memory _stakeList, uint256 _timestamp, bytes memory _signature) public stakeIsOpen {
        require(msg.sender == from, "The address of from cannot be the address of the caller");
        require(msg.sender == tx.origin, "only origin");
        uint stakeNum = _stakeList.length;
        require(stakeNum >= stakeLimitMin && stakeNum <= stakeLimitMax, "Stake Num Limit Exceeded");

        address signerOwner = signatureStake(from, _stakeList, _timestamp, _signature);
        require(signerOwner == whitelistAddress, "signer is not the executor");

        require(_timestamp > block.timestamp, "The sign be overdue");

        StakeObj storage stake = _stakeMap[from];
        require(stake.lockTime == 0, "The address has staked");
        for (uint i = 0; i < stakeNum; i++) {
            uint256 tokenId = _stakeList[i];
            require(from == whiko.ownerOf(tokenId), "The tokens owner is not from");
        }
        stake.lockTime = block.timestamp;
        stake.unLockTime = stake.lockTime.add(stakeLockTime);
        stake.lockList = _stakeList;
        _stakeMap[from] = stake;
        stakeTotal = stakeTotal.add(stakeNum);
        stakeNumber = stakeNumber.add(1);
        for (uint i = 0; i < stakeNum; i++) {
            uint256 tokenId = _stakeList[i];
            whiko.transferFrom(from, address(this), tokenId);
        }

        emit StakeNftEvent(from, _stakeList, stake.lockTime, stake.unLockTime);
    }

    function signatureStake(address from, uint256[] memory _stakeList, uint256 _timestamp, bytes memory _signature) public pure returns (address) {
        return ECDSA.recover(keccak256(abi.encode(from, _stakeList, _timestamp)), _signature);
    }

    function claim() public stakeIsOpen {
        address to = msg.sender;
        StakeObj memory stake = _stakeMap[to];
        require(stake.lockTime != 0, "The address has not stake");
        require(stake.unLockTime < block.timestamp, "The  stake Unexpired");

        uint256[] memory lockList = stake.lockList;
        uint stakeNum = lockList.length;
        for (uint i = 0; i < stakeNum; i++) {
            uint256 tokenId = lockList[i];
            whiko.transferFrom(address(this), to, tokenId);
        }
        delete _stakeMap[to];
        stakeTotal = stakeTotal.sub(stakeNum);
        stakeNumber = stakeNumber.sub(1);
        emit ClaimNftEvent(to, lockList);
    }

    function getStakeReward(address from) public view returns (uint256) {
        StakeObj memory stake = _stakeMap[from];
        require(stake.lockTime != 0, "The address has not stake");
        uint256[] memory lockList = stake.lockList;
        uint stakeNum = lockList.length;
        if (block.timestamp >= stake.unLockTime) {
            return stakeNum.mul(bonusTimes);
        }
        return block.timestamp.sub(stake.lockTime).div(stake.unLockTime.sub(stake.lockTime)).mul(stakeNum.mul(bonusTimes));
    }

    function forceClaim(address lockAddress) public stakeIsOpen isExecutor {
        StakeObj memory stake = _stakeMap[lockAddress];
        uint256[] memory lockList = stake.lockList;
        uint stakeNum = lockList.length;
        for (uint i = 0; i < stakeNum; i++) {
            uint256 tokenId = lockList[i];
            whiko.transferFrom(address(this), lockAddress, tokenId);
        }
        delete _stakeMap[lockAddress];
        stakeTotal = stakeTotal.sub(stakeNum);
        stakeNumber = stakeNumber.sub(1);
        emit ClaimNftEvent(lockAddress, lockList);
    }

    function setPause(bool _pause) public onlyOwner {
        PAUSE = _pause;
        emit PauseEvent(PAUSE);
    }
}