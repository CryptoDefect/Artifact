    /**
     * @dev This function is called from offchain code
     * It is called when the NFT is sold for first time.
     * It is to be used when ETH is to be transferred to collaborators
     * It will send amount passed in msg.value to collaborators.
     * Note this amount may not be the actual sale amount paid by the buyer.
     * Buyer pays (SalePrice + gas)
     * OpenSea keeps 2.5% * SalePrice as its fees
     * OpenSea sends (SaleRoyalty%) * SalePrice to royalty receiver
     * OpenSea sends remaining amount to seller
     * We assume we are receving the "remaining amount" and hence send all of it to the collaborator.
     */
    function distributeInitialSaleRevenue(uint256 tokenId)
        external
        payable
        onlyOwner
    {
        require(ownerOf(tokenId) != address(0), "Token does not exist");
        address payable collaborator = payable(
            tokenCollaborator[tokenId].splitAddress
        );

        //Send amount that has come in msg.value to collaborator
        (bool sent, ) = collaborator.call{value: msg.value}("");
        console.log("***Amount sent to collaborator", msg.value);
        require(sent, "Failed to send Ether");
    }

    /**
    * @dev This function is called from offchain code
    * It is called when any streaming revenue is received for a token.  
    i.	If initial sale has not happened
    •	  It will send amount*(1-streamingRoyalty) to creators
    •	  It will send amount*streamingRoyalty to contract owner (ommuse address)
    ii.	If initial sale has happened
    •	  It will send amount*50% to current owner
    •	  It will send amount*50%*(1-streamingRoyalty) to creators
    •	  It will send amount*50%*streamingRoyalty to OmMuse
        Suppose amount =200 and streamingRoyalty=15% or 0.15
        then 50% or 100 goes to owner (200*(0.5) = 100)
        15% of 100 or 15 goes to ommuse (200*0.5*0.15 = 15)
        85% of 100 or 85 goes to creators (200*0.5*0.85 = 85)

    */
    function distributeStreamingRevenue(uint256 tokenId)
        external
        payable
        onlyOwner
    {
        address payable ownerOfToken = payable(ownerOf(tokenId));
        require(ownerOfToken != address(0), "Token does not exist");
        address payable collaborator = payable(
            tokenCollaborator[tokenId].splitAddress
        );
        uint256 amount = msg.value; //total amount received
        console.log("***Amount received", amount);
        bool sent;

        //Get how much portion of the amount is streaming royalty
        (address royaltyReceiver, uint256 royaltyAmount) = streamingRoyaltyInfo(
            tokenId,
            amount
        );
        // prettier-ignore
        if (ownerOfToken == owner()) //Ommuse is the owner, meaning first token sale yet to happen
        //So entire streaming royalty (15%) goes to contractOwner(ommuse)
        //and remaining amount (85%) goes to collaborators
        {
            //Send 15% to royaltyReceiver which is expected to be contractOwner, OmMuse
            uint256 contractOwnerRoyaltyAmount;
            (royaltyReceiver, contractOwnerRoyaltyAmount) = streamingRoyaltyInfo(tokenId, (amount));
            console.log("***Amount sent to contractOwner", contractOwnerRoyaltyAmount);

            (sent, ) = payable(royaltyReceiver).call{value: (contractOwnerRoyaltyAmount)}("");
            require(sent, "Failed to send Ether to contractOwner");

            //Send 85% to collaborator split address
            (sent, ) = collaborator.call{value: (amount - contractOwnerRoyaltyAmount)}("");
            console.log( "***Amount sent to collaborator",amount - contractOwnerRoyaltyAmount);
            require(sent, "Failed to send Ether to collaborator");
        } else {
            //token has already been sold
            //Send 50% to current Owner 
            //Remaining 50% is split between Collaborators and OmMuse (85%,15%) if royalty is 15%

            //Sending to current owner
            (sent, ) = ownerOfToken.call{value: (amount - royaltyAmount)}("");
            require(sent, "Failed to send Ether to owner");
            console.log("***Amount sent to owner", (amount - royaltyAmount));

            //Send 15% of the 15% to royaltyReceiver which is expected to be contractOwner, OmMuse
            uint256 contractOwnerRoyaltyAmount;
            (royaltyReceiver, contractOwnerRoyaltyAmount) = streamingRoyaltyInfo(tokenId, royaltyAmount);

            (sent, ) = payable(royaltyReceiver).call{value: (contractOwnerRoyaltyAmount)}("");
            console.log( "***Amount sent to contractOwner", (contractOwnerRoyaltyAmount));

            require(sent, "Failed to send Ether to contractOwner");

            //Send 85% of the 15% to collaborator split address
            (sent, ) = collaborator.call{value: (royaltyAmount - contractOwnerRoyaltyAmount)}("");
            console.log( "***Amount sent to collaborator", (royaltyAmount - contractOwnerRoyaltyAmount));
            require(sent, "Failed to send Ether to collaborator");
        }
        // prettier-ignore
    }

