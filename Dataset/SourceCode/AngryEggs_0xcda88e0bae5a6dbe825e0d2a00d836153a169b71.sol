// File contracts/AngryEggs.sol

// Creators: Chiru Labs

pragma solidity ^0.8.4;





contract AngryEggs is ERC721A, Ownable, ReentrancyGuard{
    using Strings for uint256;

    uint256 public price;
    uint256 public reserved;
    string private _baseTokenURI;
    address public treasury;
    IRandomNumberGenerator public randomGenerator;
    bool private randomNumberReqSent;
    uint256 public immutable maxSupply;

    constructor(
        uint256 _price,
        uint256 _reserved,
        address _treasury,
        uint256 _maxSupply
    ) ERC721A("The Angry Eggs", "ANGRYEGG") {
        price = _price;
        reserved = _reserved;
        treasury = _treasury;
        maxSupply= _maxSupply;

        _mint(treasury, reserved, '0x0', true);
    }

    function setRandomGenerator(address _randomGenerator) public onlyOwner {
        randomGenerator = IRandomNumberGenerator(_randomGenerator);
    }

    function numberMinted(address owner) public view returns (uint256) {
        return _numberMinted(owner);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string calldata baseURI) external onlyOwner {
        _baseTokenURI = baseURI;
    }

    function exists(uint256 tokenId) public view returns (bool) {
        return _exists(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        if(randomGenerator.randomNumber() == 0){
            return _baseTokenURI;
        }
        require(_exists(tokenId), "nonexistent token");
        uint256 tokenPath = (startingIndex() + tokenId) % maxSupply;

        return string(abi.encodePacked(_baseTokenURI, tokenPath.toString()));
    }

    function transferFunds(uint256 totalCost) private {
        require(msg.value == totalCost, "wrong amount");
        (bool success, ) = payable(treasury).call{value: totalCost}("");
        require(success, "transfer failed");
    }

    function mint(
        uint256 quantity
    ) public payable {
        require(totalSupply() + quantity <= maxSupply, "max supply reached");
        uint256 totalCost = price * quantity;
        require(msg.value == totalCost, "Wrong amount sent");
        transferFunds(totalCost);
        _mint(msg.sender, quantity, '', true);
    }

    function startingIndex() public view returns(uint256) {
        return randomGenerator.randomNumber();
    }

    function reveal() external onlyOwner {
        if (!randomNumberReqSent) {
            randomNumberReqSent = true;
            randomGenerator.getRandomNumber();
        }
    }
}


