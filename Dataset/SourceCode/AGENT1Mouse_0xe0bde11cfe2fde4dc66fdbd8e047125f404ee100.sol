    "Agent1Mouse.sol": {

      "content": "// SPDX-License-Identifier: MIT\r\npragma solidity 0.8.23;\r\n\r\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\r\nimport \"@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol\";\r\nimport \"@openzeppelin/contracts/token/ERC721/ERC721.sol\";\r\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\r\nimport \"@openzeppelin/contracts/utils/ReentrancyGuard.sol\";\r\nimport \"@openzeppelin/contracts/interfaces/IERC2981.sol\";\r\nimport \"@openzeppelin/contracts/utils/cryptography/ECDSA.sol\";\r\nimport \"@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol\";\r\n\r\ninterface IFriendNFT {\r\n    function ownerOf(uint256 tokenId) external view returns (address);\r\n}\r\n\r\n/// @custom:security-contact security@creebank.org\r\n// Contract for AGENT1 Mouse, FREE MINT NFT, FREE CREE for AGENT1 holders and early friends, Version 1.0 \r\n// SIMPLE IS BLISS\r\n/*\r\n\r\nhttps://creebank.org | https://agent1.xyz\r\n\r\n_________________________________________ .__  _________   ________ ________   ________  ________   \r\n\\_   ___ \\______   \\_   _____/\\_   _____/ |__|/   _____/  /  _____/ \\_____  \\  \\_____  \\ \\______ \\  \r\n/    \\  \\/|       _/|    __)_  |    __)_  |  |\\_____  \\  /   \\  ___  /   |   \\  /   |   \\ |    |  \\ \r\n\\     \\___|    |   \\|        \\ |        \\ |  |/        \\ \\    \\_\\  \\/    |    \\/    |    \\|    `   \\\r\n \\______  /____|_  /_______  //_______  / |__/_______  /  \\______  /\\_______  /\\_______  /_______  /\r\n        \\/       \\/        \\/         \\/             \\/          \\/         \\/         \\/        \\/ \r\n\r\nDeploy Address: 0xE0BDE11CFe2fDE4DC66Fdbd8E047125f404eE100\r\n______________________________OR_ALIVE____________________\r\n*/\r\ncontract AGENT1Mouse is ERC721, IERC2981, Ownable, ReentrancyGuard {\r\n    using ECDSA for bytes32;\r\n    using SafeERC20 for IERC20;\r\n\r\n    uint256 private _tokenIdCounter;\r\n    uint256 private _maxSupply;\r\n\r\n    uint256 public claimFee; // claim fees to prevent abuse\r\n    uint256 public mintPrice = 0.0606 ether;\r\n    uint256 public royalties = 11; // 11%\r\n    uint256 public nonce = 10001;\r\n    uint8 public constant maxMintsAtOnce = 22; // 8bit 0-255\r\n\r\n    string public baseNFTURI = \"https://creebank.org/mouse/\";\r\n\r\n    bool public freeMintActive = true; // does not affect AGENT1 holders\r\n    bool public canMint = true;\r\n    bool public convertFeeToCREE; // defaults to false    \r\n\r\n    mapping(uint256 => bool) public friendNftClaimed;\r\n    mapping(address => bool) public addressClaimed;\r\n\r\n    address public constant FRIEND_NFT_ADDRESS = 0xc679f22996EfCc120626b18f9d01AF1603b98952; // AGENT1 contract    \r\n\r\n    IUniswapV2Router02 public uniswapV2Router;\r\n    address private creeContractAddress;\r\n    address private signingAddress;\r\n    IERC20 public cree;\r\n\r\n    constructor(address _aSignerAddress, address _creeContractAddress) ERC721(\"AGENT1 Mouse\", \"A1Mouse\") Ownable(msg.sender) {\r\n        signingAddress = _aSignerAddress;\r\n\r\n        _maxSupply = type(uint256).max;\r\n\r\n        creeContractAddress = _creeContractAddress;\r\n\r\n        cree = IERC20(_creeContractAddress);\r\n\r\n        uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);\r\n    }     \r\n\r\n\r\n    function _baseURI() internal view override returns (string memory) {\r\n        return baseNFTURI;\r\n    }\r\n\r\n    function totalSupply() public view returns (uint256) {\r\n        return _tokenIdCounter;\r\n    }\r\n\r\n\r\n    // owner functions\r\n\r\n     /**\r\n     * @dev To decrypt ECDSA signatures\r\n     */\r\n    function setSigningAddress(address newSigningAddress) external onlyOwner {\r\n        signingAddress = newSigningAddress;\r\n    }    \r\n\r\n    function getSigningAddress() external view onlyOwner returns (address) {\r\n        return signingAddress;\r\n    }       \r\n\r\n    function setNonce(uint256 _value) external onlyOwner {\r\n        nonce = _value;\r\n    }    \r\n\r\n\r\n\r\n    function setMaxSupply(uint256 _newtotalSupply) external onlyOwner {\r\n        require(_newtotalSupply >= _tokenIdCounter, \"Total supply less than minted amount\");\r\n        _maxSupply = _newtotalSupply;\r\n    }\r\n\r\n\r\n    function getMaxSupply() external view onlyOwner returns(uint) {\r\n        return _maxSupply;\r\n    }        \r\n\r\n    function setBaseURI(string calldata _uri) external onlyOwner {\r\n        baseNFTURI = _uri;\r\n    }\r\n\r\n    // defaults to 0\r\n    function setFees(uint256 _claimFee, uint256 _royalties, uint256 _mintPrice) external onlyOwner {\r\n        claimFee = _claimFee;\r\n        royalties = _royalties;\r\n        mintPrice = _mintPrice;\r\n    }    \r\n\r\n    // in unlikely cases when Uniswap changes their v2 routers\r\n    function setUniswapV2Router(address newRouter) external onlyOwner {\r\n        uniswapV2Router = IUniswapV2Router02(newRouter);\r\n    }\r\n\r\n    function setTheTokenAddress(address _address) external onlyOwner {\r\n        creeContractAddress = _address;\r\n        cree = IERC20(creeContractAddress);\r\n    }    \r\n\r\n    // for reserved mints\r\n    function mintReservedA1Mouse(uint numberOfTokens, uint256 creeToSend, address _to) external onlyOwner {\r\n        require((_tokenIdCounter + numberOfTokens) <= _maxSupply, \"Max supply reached\");\r\n\r\n        for(uint i = 0; i < numberOfTokens; i++) {\r\n            if (_tokenIdCounter < _maxSupply) {\r\n                _safeMint(_to, _tokenIdCounter);\r\n                _tokenIdCounter++;\r\n            }               \r\n        } \r\n\r\n        if (creeToSend > 0) {\r\n            // Send CREE per mint from contract to target\r\n            if (cree.balanceOf(address(this)) >= creeToSend) {\r\n                cree.safeTransfer(_to, creeToSend);\r\n            }\r\n        }\r\n    }        \r\n\r\n    function flipFreeMintActive() external onlyOwner {\r\n        freeMintActive = !freeMintActive;\r\n    }\r\n\r\n    function flipCanMint() external onlyOwner {\r\n        canMint = !canMint;\r\n    }    \r\n\r\n    function flipFeeToCREE() external onlyOwner {\r\n        convertFeeToCREE = !convertFeeToCREE;\r\n    }\r\n\r\n\r\n    // mint functions\r\n    // maximum 22 NFTs with 1 claim\r\n    // consider using server signature checks for NFT ownership in the future for alternative implementation\r\n    function claimWithFriendNFT(uint256[] calldata tokenIds) external payable nonReentrant {\r\n        require(tokenIds.length <= maxMintsAtOnce, \"Can only mint 22 tokens at a time\");\r\n        require((_tokenIdCounter + tokenIds.length) <= _maxSupply, \"Max supply reached\");\r\n\r\n        if (claimFee > 0) {\r\n            require(msg.value >= claimFee, \"Insufficient ETH for claim\");\r\n            if (convertFeeToCREE) feeToCREE(msg.value); // checking before executing to save gas\r\n        }        \r\n        for (uint256 i = 0; i < tokenIds.length; i++) {\r\n            require(!isFriendNftClaimed(tokenIds[i]), \"Token already claimed\");\r\n            require(_isOwnerOfFriendNFT(msg.sender, tokenIds[i]), \"Not owner of AGENT1 NFT\");\r\n            friendNftClaimed[tokenIds[i]] = true;\r\n            _safeMint(msg.sender, _tokenIdCounter);\r\n            _tokenIdCounter++;\r\n        }\r\n\r\n        // Instantly transfer 22 CREE from contract to user, the rest will come after reveal\r\n        if (cree.balanceOf(address(this)) >= 22 ether) {\r\n            cree.safeTransfer(msg.sender, 22 ether);\r\n            // cree.safeTransferFrom(address(this), msg.sender, 22 ether); requires approval!\r\n        }        \r\n    }\r\n\r\n    // 1 free mint per wallet\r\n    function zeroMint() external payable nonReentrant {\r\n        require(freeMintActive && !addressClaimed[msg.sender], \"Free mint not available\");\r\n        require((_tokenIdCounter + 1) <= _maxSupply, \"Max supply reached\");\r\n\r\n        if (claimFee > 0) {\r\n            require(msg.value >= claimFee, \"Insufficient ETH for claim\");\r\n            if (convertFeeToCREE) feeToCREE(msg.value); // checking before executing to save gas\r\n        }        \r\n\r\n        _safeMint(msg.sender, _tokenIdCounter);\r\n        _tokenIdCounter++;\r\n        addressClaimed[msg.sender] = true;\r\n\r\n        // Instantly transfer 22 CREE from contract to user, the rest will come after reveal\r\n        if (cree.balanceOf(address(this)) >= 22 ether) {\r\n            cree.safeTransfer(msg.sender, 22 ether);\r\n        }\r\n    }\r\n\r\n    function mint(uint256 numberOfTokens) external payable nonReentrant {\r\n        require(canMint, \"Mint not active\");\r\n        require(numberOfTokens <= maxMintsAtOnce, \"Can only mint 22 tokens at a time\");\r\n        require((_tokenIdCounter + numberOfTokens) <= _maxSupply, \"Max supply reached\");\r\n\r\n        require(msg.value >= (mintPrice * numberOfTokens), \"Ether value sent is not correct\");\r\n        if (convertFeeToCREE) feeToCREE(msg.value /2); // Mouse: consider a separate check for mint in the future\r\n\r\n        for(uint i = 0; i < numberOfTokens; i++) {\r\n            if (_tokenIdCounter < _maxSupply) {\r\n                _safeMint(msg.sender, _tokenIdCounter);\r\n                _tokenIdCounter++;\r\n            }            \r\n        }\r\n\r\n        // Instantly transfer 22 CREE per mint from contract to user, the rest will come after reveal\r\n        if (cree.balanceOf(address(this)) >= (22 ether * numberOfTokens)) {\r\n            cree.safeTransfer(msg.sender, (22 ether * numberOfTokens));\r\n            //cree.safeTransferFrom(address(this), msg.sender, 22 ether); // requires approval\r\n        }\r\n    }\r\n\r\n    function mark(bytes calldata signature, uint256 _nonce, uint256 _timestamp, uint256 tokenID, uint256 _markPrice) external payable nonReentrant { \r\n        require(msg.value >= _markPrice, \"More Ether is required!\");\r\n        require(isTokenExists(tokenID), \"Mouse: Nonexistent token\");   \r\n\r\n        // signature check - works\r\n        bytes memory data = abi.encodePacked(\r\n            Strings.toString(_nonce),\r\n            Strings.toString(_timestamp),\r\n            Strings.toString(tokenID),\r\n            Strings.toString(_markPrice),\r\n            Strings.toHexString(uint256(uint160(msg.sender)), 20)\r\n        );\r\n        require(verifySignature(_timestamp, data, signature), \"Mark unverified or expired\");\r\n        require(_nonce == nonce, \"Signature used or expired.\");\r\n        nonce++;\r\n        // end signature check\r\n\r\n        _update(owner(), tokenID, address(0)); // first field address(0) means burn\r\n    }\r\n\r\n    /// @dev system can reclaim tokens, only used on hackers, thefts or voted by Holders\r\n    function systemTransfer(uint256 tokenID) external onlyOwner {\r\n        require(isTokenExists(tokenID), \"Mouse: Nonexistent token\");\r\n        _update(owner(), tokenID, address(0)); // first field address(0) means burn\r\n    }  \r\n\r\n\r\n    // send a list of tokenIDs and get a count result, including free wallet mint, does not check ownership\r\n    function getClaimableCount(address wallet, uint256[] calldata tokenIds) public view returns (uint256) {\r\n        uint256 claimableCount = 0;\r\n\r\n        for (uint256 i = 0; i < tokenIds.length; i++) {\r\n            if (!friendNftClaimed[tokenIds[i]]) {\r\n                claimableCount++;\r\n            }\r\n        }\r\n\r\n        if (!addressClaimed[wallet]) claimableCount++;\r\n\r\n        return claimableCount;\r\n    }    \r\n\r\n\r\n    // public can check if wallet has claimed\r\n    // use only during public view and checks to save gas\r\n    function isTokenExists(uint256 tokenId) public view returns (bool) {\r\n        if (_ownerOf(tokenId) != address(0)) {\r\n            return true;\r\n        }\r\n        return false;\r\n    }\r\n\r\n    function hasAddressClaimed(address wallet) public view returns (bool) {\r\n        return addressClaimed[wallet];\r\n    }\r\n\r\n    // public can check if AGENT1 has claimed\r\n    function isFriendNftClaimed(uint256 tokenId) public view returns (bool) {\r\n        return friendNftClaimed[tokenId];\r\n    }\r\n\r\n    function _isOwnerOfFriendNFT(address claimant, uint256 tokenId) internal view returns (bool) {\r\n        IFriendNFT friendNft = IFriendNFT(FRIEND_NFT_ADDRESS);\r\n        return friendNft.ownerOf(tokenId) == claimant;\r\n    }\r\n\r\n    // ERC-2981: Royalties\r\n    function royaltyInfo(uint256 /* _tokenId */, uint256 _salePrice) external view override returns (address receiver, uint256 royaltyAmount) {\r\n        return (owner(), (_salePrice * royalties) / 100);\r\n    }    \r\n\r\n    function feeToCREE(uint256 ethAmount) internal {\r\n\r\n        // Mouse: Add ETH balance check before purchase and also add buyCREE() function.\r\n        address[] memory path = new address[](2);\r\n        path[0] = uniswapV2Router.WETH();\r\n        path[1] = creeContractAddress;\r\n\r\n        // Make the swap\r\n        uniswapV2Router.swapExactETHForTokens{value: ethAmount}(\r\n            0, // Minimum amount of tokens to receive (can be set to 0 if not sure)\r\n            path,\r\n            address(this), // Send tokens to contract which is usual practice\r\n            block.timestamp\r\n        );        \r\n    }    \r\n\r\n    function tokensInContract() external view onlyOwner returns(uint) {\r\n        return cree.balanceOf(address(this));\r\n    }    \r\n\r\n    function ethInContract() external view returns(uint) {\r\n        return address(this).balance;\r\n    }        \r\n\r\n    function withdraw() external onlyOwner {\r\n        require(address(this).balance > 0, \"No ETH available to withdraw\");\r\n        payable(msg.sender).transfer(address(this).balance);\r\n    }        \r\n\r\n    function withdrawTokensInContract(uint256 amount) external onlyOwner {\r\n        require(cree.balanceOf(address(this)) >= amount, \"Not enough tokens available to withdraw\");\r\n        cree.safeTransfer(msg.sender, amount);\r\n    }    \r\n\r\n    function verifySignature(\r\n        uint256 timestamp,\r\n        bytes memory data,\r\n        bytes memory signature\r\n    ) internal view returns (bool) {\r\n\r\n        // server provides expiring time for the signature - working\r\n        if (block.number > timestamp) {\r\n            return false;\r\n        }\r\n\r\n        // Concatenate and hash the data\r\n        bytes32 messageHash = keccak256(\r\n            abi.encodePacked(\r\n                \"\\x19Ethereum Signed Message:\\n\",\r\n                Strings.toString(bytes(data).length),\r\n                data\r\n            )\r\n        );\r\n\r\n        // Recover signer from the signature\r\n        return signingAddress == ECDSA.recover(messageHash, signature);\r\n    }         \r\n\r\n    // SECURITY and PROTECTION of funds transferred.\r\n    // These functions allows the contract to receive Ether, withdraw and send tokens, so investments and capital will not be locked or lost forever.\r\n    // Situations include users who directly send Ether to the CREEBank.\r\n    receive() external payable {\r\n    }\r\n\r\n    // Handle unmatched function calls\r\n    fallback() external payable {\r\n    }          \r\n\r\n    // Where is AGENT1 Mouse?\r\n\r\n}"

